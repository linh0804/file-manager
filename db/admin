auto_increment(){return" PRIMARY KEY AUTOINCREMENT";}function
alter_table($R,$A,$m,$he,$Ob,$nd,$Fb,$Ra,$Yh){$Ul=($R==""||$he);foreach($m
as$l){if($l[0]!=""||!$l[1]||$l[2]){$Ul=true;break;}}$Ia=[];$Kh=[];foreach($m
as$l){if(!$l[1])continue;if($l[0]!="")$Kh[$l[0]]=$l[1][0];$Ia[]=($Ul?$l[1]:"ADD ".implode($l[1]));}if(!$Ul){foreach($Ia
as$X){if(!queries("ALTER TABLE ".table($R)." $X"))return
false;}if($R!=$A&&!queries("ALTER TABLE ".table($R)." RENAME TO ".table($A)))return
false;}elseif(!recreate_table($R,$A,$Ia,$Kh,$he,$Ra))return
false;if($Ra){queries("BEGIN");queries("UPDATE sqlite_sequence SET seq = $Ra WHERE name = ".q($A));if(!Connection::get()->getAffectedRows())queries("INSERT INTO sqlite_sequence (name, seq) VALUES (".q($A).", $Ra)");queries("COMMIT");}return
true;}function
recreate_table($R,$A,$m,$Kh,$he,$Ra=0,$v=[],$Vc="",$va=""){if($R!=""){if(!$m){foreach(fields($R)as$w=>$l){if($v)$l["auto_increment"]=0;$m[]=process_field($l,$l);$Kh[$w]=idf_escape($w);}}$xi=false;foreach($m
as$l){if($l[6])$xi=true;}$Xc=[];foreach($v
as$w=>$X){if($X[2]=="DROP"){$Xc[$X[1]]=true;unset($v[$w]);}}foreach(indexes($R)as$Hf=>$u){$e=[];foreach($u["columns"]as$w=>$d){if(!isset($Kh[$d]))continue
2;$e[]=$Kh[$d].($u["descs"][$w]?" DESC":"");}if(!$Xc[$Hf]){if($u["type"]!="PRIMARY"||!$xi){$Hf=preg_replace('~^sqlite_~',"",$Hf);$v[]=[$u["type"],$Hf,$e];}}}foreach($v
as$w=>$X){if($X[0]=="PRIMARY"){unset($v[$w]);$he[]="  PRIMARY KEY (".implode(", ",$X[2]).")";}}foreach(foreign_keys($R)as$Hf=>$p){foreach($p["source"]as$w=>$d){if(!$Kh[$d])continue
2;$p["source"][$w]=idf_unescape($Kh[$d]);}if(!isset($he[" $Hf"]))$he[]=" ".format_foreign_key($p);}queries("BEGIN");}foreach($m
as$w=>$l){if(preg_match('~GENERATED~',$l[3]))unset($Kh[array_search($l[0],$Kh)]);$m[$w]="  ".implode($l);}$m=array_merge($m,array_filter($he));foreach(Driver::get()->checkConstraints($R)as$rb){if($rb!=$Vc)$m[]="  CHECK ($rb)";}if($va)$m[]="  CHECK ($va)";$Zk=($R==$A?"adminneo_$A":$A);if(!queries("CREATE TABLE ".table($Zk)." (\n".implode(",\n",$m)."\n)"))return
false;if($R!=""){if($Kh&&!queries("INSERT INTO ".table($Zk)." (".implode(", ",$Kh).") SELECT ".implode(", ",array_map('AdminNeo\idf_escape',array_keys($Kh)))." FROM ".table($R)))return
false;$Bl=[];foreach(triggers($R)as$_l=>$kl){$zl=trigger($_l);$Bl[]="CREATE TRIGGER ".idf_escape($_l)." ".implode(" ",$kl)." ON ".table($A)."\n$zl[Statement]";}$Ra=$Ra?0:Connection::get()->getValue("SELECT seq FROM sqlite_sequence WHERE name = ".q($R));if(!queries("DROP TABLE ".table($R))||($R==$A&&!queries("ALTER TABLE ".table($Zk)." RENAME TO ".table($A)))||!alter_indexes($A,$v))return
false;if($Ra)queries("UPDATE sqlite_sequence SET seq = $Ra WHERE name = ".q($A));foreach($Bl
as$zl){if(!queries($zl))return
false;}queries("COMMIT");}return
true;}function
index_sql($R,$U,$A,$e){return"CREATE $U ".($U!="INDEX"?"INDEX ":"").idf_escape($A!=""?$A:uniqid($R."_"))." ON ".table($R)." $e";}function
alter_indexes($R,$c){foreach($c
as$u){if($u[0]=="PRIMARY"||(preg_match('~^sqlite_~',$u[1])))return
recreate_table($R,$R,[],[],[],0,$c);}foreach(array_reverse($c)as$X){if(!queries($X[2]=="DROP"?"DROP INDEX ".idf_escape($X[1]):index_sql($R,$X[0],$X[1],"(".implode(", ",$X[2]).")")))return
false;}return
true;}function
