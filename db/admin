function
filterListWithWildcards(array$Jl,array$Wd,$Af,array$rk=[]){if(!$Jl||!$Wd)return$Jl;$u=array_search("__system",$Wd);if($u!==false){unset($Wd[$u]);$Wd=array_merge($Wd,$rk);}array_walk($Wd,function(&$Y){$Y=str_replace('\\*',".*",preg_quote($Y,"~"));});$Uh='~^('.implode("|",$Wd).')$~';return$this->filterListWithPattern($Jl,$Uh,$Af);}private
function
filterListWithPattern(array$Jl,$Uh,$Af){$G=[];foreach($Jl
as$x=>$Y){if(is_array($Y)){if($hk=$this->filterListWithPattern($Y,$Uh,$Af))$G[$x]=$hk;}elseif(($Af&&preg_match($Uh,$Y))||(!$Af&&!preg_match($Uh,$Y)))$G[$x]=$Y;}return$G;}abstract
