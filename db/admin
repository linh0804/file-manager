function
create(array$Yb=[],array$ni=[]){if(self::$instance)die("Admin instance already exists.\n");$b=new
static();if(!$Yb&&file_exists("adminneo-config.php")){$Yb=include_once("adminneo-config.php");if(!is_array($Yb)){$Yb=[];$dg="href=https://github.com/adminneo-org/adminneo#configuration ".target_blank();$b->addError(lang(0,"<b>adminneo-config.php</b>")." <a $dg>".lang(1)."</a>");}}$Yb=new
Config($Yb);$P=new
