printSelectionColumns(array$L,array$e){print_fieldset_start("select",lang(35),"columns",(bool)$L,true);$_GET["columns"][""]=[];$r=0;foreach($_GET["columns"]as$w=>$X){if($w!=""&&(isset($X["col"])?$X["col"]:null)=="")continue;$d=select_input("name='columns[$r][col]'",$e,isset($X["col"])?$X["col"]:null,$w!==""?"selectFieldChange":"selectAddRow");echo"<div ",($w!=""?"":"class='no-sort'"),">",icon("handle","handle jsonly");if(Driver::get()->getFunctions()||Driver::get()->getGrouping())echo
