fields($R){$ng=Connection::get()->isMariaDB();$K=[];foreach(get_rows("SELECT * FROM information_schema.COLUMNS WHERE TABLE_SCHEMA = DATABASE() AND TABLE_NAME = ".q($R)." ORDER BY ORDINAL_POSITION")as$L){$l=$L["COLUMN_NAME"];$U=preg_replace('~\s?/\*.+\*/~U',"",$L["COLUMN_TYPE"]);$Ld=$L["EXTRA"];preg_match('~^(VIRTUAL|PERSISTENT|STORED)~',$Ld,$ve);preg_match('~^([^( ]+)(?:\((.+)\))?( unsigned)?( zerofill)?$~',$U,$Il);$j=$ng&&$L["COLUMN_DEFAULT"]=="NULL"?null:$L["COLUMN_DEFAULT"];if($j!==null){$yf=preg_match('~(text|json)~',$Il[1]);if(!$ng&&$yf)$j=preg_replace("~^(_\w+)?('.*')$~",'\2',stripslashes($j));if($ng||$yf){$j=preg_replace_callback("~^'(.*)'$~",function($_){return
stripslashes(str_replace("''","'",$_[1]));},$j);}if(!$ng&&preg_match('~binary~',$Il[1])&&preg_match('~^0x(\w*)$~',$j,$_))$j=pack("H*",$_[1]);}$xe=$L["GENERATION_EXPRESSION"];if(!$ng)$xe=preg_replace("~(^|,|\()(_\w+)?('.*')($|,|\))~",'\1\3\4',stripslashes($xe));$K[$l]=["field"=>$l,"full_type"=>$U,"type"=>$Il[1],"length"=>$Il[2],"unsigned"=>ltrim($Il[3].$Il[4]),"default"=>($ve?$xe:$j),"null"=>($L["IS_NULLABLE"]=="YES"),"auto_increment"=>($Ld=="auto_increment"),"on_update"=>(preg_match('~\bon update (\w+)~i',$Ld,$Il)?$Il[1]:""),"collation"=>$L["COLLATION_NAME"],"privileges"=>array_flip(explode(",",$L["PRIVILEGES"]))+["where"=>1,"order"=>1],"comment"=>$L["COLUMN_COMMENT"],"primary"=>($L["COLUMN_KEY"]=="PRI"),"generated"=>($ve[1]=="PERSISTENT"?"STORED":$ve[1]),];}return$K;}function
indexes($R,$f=null){$K=[];foreach(get_rows("SHOW INDEX FROM ".table($R),$f)as$L){$A=$L["Key_name"];$K[$A]["type"]=($A=="PRIMARY"?"PRIMARY":($L["Index_type"]=="FULLTEXT"?"FULLTEXT":($L["Non_unique"]?($L["Index_type"]=="SPATIAL"?"SPATIAL":"INDEX"):"UNIQUE")));$K[$A]["columns"][]=$L["Column_name"];$K[$A]["lengths"][]=($L["Index_type"]=="SPATIAL"?null:$L["Sub_part"]);$K[$A]["descs"][]=null;}return$K;}function
foreign_keys($R){static$hi='(?:`(?:[^`]|``)+`|"(?:[^"]|"")+")';$K=[];$nc=Connection::get()->getValue("SHOW CREATE TABLE ".table($R),1);if($nc){$rh=implode("|",Driver::get()->getOnActions());preg_match_all("~CONSTRAINT ($hi) FOREIGN KEY ?\\(((?:$hi,? ?)+)\\) REFERENCES ($hi)(?:\\.($hi))? \\(((?:$hi,? ?)+)\\)(?: ON DELETE ($rh))?(?: ON UPDATE ($rh))?~",$nc,$_,PREG_SET_ORDER);foreach($_
as$z){preg_match_all("~$hi~",$z[2],$ik);preg_match_all("~$hi~",$z[5],$al);$K[idf_unescape($z[1])]=["db"=>idf_unescape($z[4]!=""?$z[3]:$z[4]),"table"=>idf_unescape($z[4]!=""?$z[4]:$z[3]),"source"=>array_map('AdminNeo\idf_unescape',$ik[0]),"target"=>array_map('AdminNeo\idf_unescape',$al[0]),"on_delete"=>($z[6]?:"RESTRICT"),"on_update"=>($z[7]?:"RESTRICT"),];}}return$K;}function
view($A){return["select"=>preg_replace('~^(?:[^`]|`[^`]*`)*\s+AS\s+~isU','',Connection::get()->getValue("SHOW CREATE VIEW ".table($A),1))];}function
collations(){$K=[];$H=Connection::get()->isMariaDB()&&Connection::get()->isMinVersion("10.10")?"SELECT CHARACTER_SET_NAME AS Charset, FULL_COLLATION_NAME AS Collation, IS_DEFAULT AS `Default` FROM information_schema.COLLATION_CHARACTER_SET_APPLICABILITY":"SHOW COLLATION";foreach(get_rows($H)as$L){if($L["Default"])$K[$L["Charset"]][-1]=$L["Collation"];else$K[$L["Charset"]][]=$L["Collation"];}ksort($K);foreach($K
as$w=>$X)asort($K[$w]);return$K;}function
