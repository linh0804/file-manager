insertUpdate($R,array$Ni,array$G){$m=fields($R);$Pl=[];$Z=[];$I=reset($Ni);$e="c".implode(", c",range(1,count($I)));$ib=0;$mf=[];foreach($I
as$w=>$X){$ib++;$A=idf_unescape($w);if(!$m[$A]["auto_increment"])$mf[$w]="c$ib";if(isset($G[$A]))$Z[]="$w = c$ib";else$Pl[]="$w = c$ib";}$cm=[];foreach($Ni
as$I)$cm[]="(".implode(", ",$I).")";if($Z){$Ye=queries("SET IDENTITY_INSERT ".table($R)." ON");$K=queries("MERGE ".table($R)." USING (VALUES\n\t".implode(",\n\t",$cm)."\n) AS source ($e) ON ".implode(" AND ",$Z).($Pl?"\nWHEN MATCHED THEN UPDATE SET ".implode(", ",$Pl):"")."\nWHEN NOT MATCHED THEN INSERT (".implode(", ",array_keys($Ye?$I:$mf)).") VALUES (".($Ye?$e:implode(", ",$mf)).");");if($Ye)queries("SET IDENTITY_INSERT ".table($R)." OFF");}else$K=queries("INSERT INTO ".table($R)." (".implode(", ",array_keys($I)).") VALUES\n".implode(",\n",$cm));return$K;}function
