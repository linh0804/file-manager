formatMessageQuery($F,$Qk,$Md=false){global$w,$j;restart_session();$Je=&get_session("queries");if(!isset($Je[$_GET["db"]]))$Je[$_GET["db"]]=[];if(strlen($F)>1e6)$F=preg_replace('~[\x80-\xFF]+$~','',substr($F,0,1e6))."\n…";$Je[$_GET["db"]][]=[$F,time(),$Qk];$nk=support("sql");$Tl=!$Md?$j->warnings():null;$Xj="sql-".count($Je[$_GET["db"]]);$Ul="warnings-".count($Je[$_GET["db"]]);$H=" ";if($Tl)$H
.="<a href='#$Ul' class='toggle'>".lang(21).icon_chevron_down()."</a>, ";$zi=support("sql")?lang(22):lang(23);$H
.="<a href='#$Xj' class='toggle'>$zi".icon_chevron_down()."</a>";$H
.=" <span class='time'>".@date("H:i:s")."</span>\n";if($Tl)$H
.="<div id='$Ul' class='warnings hidden'>\n$Tl</div>\n";$H
.="<div id='$Xj' class='hidden'>\n";$pk=$w=="elastic"?"js":$w;$H
.="<pre><code class='jush-$pk'>".truncate_utf8($F,1000)."</code></pre>\n";$H
.="<p class='links'>";if($nk)$H
.="<a href='".h(str_replace("db=".urlencode(DB),"db=".urlencode($_GET["db"]),ME).'sql=&history='.(count($Je[$_GET["db"]])-1))."'>".icon("edit").lang(20)."</a>";if($Qk)$H
.=" <span class='time'>($Qk)</span>";$H
.="</p>\n";$H
.="</div>\n";return$H;}function
formatSqlCommandQuery($F){if(preg_match('~^DELIMITER\s~i',$F))return"";return
truncate_utf8($F,1000);}function
