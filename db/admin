select($G,$f=null,$th=[],$y=0){global$w;$Yf=[];$v=[];$d=[];$lb=[];$U=[];$H=[];for($r=0;(!$y||$r<$y)&&($I=$G->fetch_row());$r++){if(!$r){echo"<div class='scrollable'>\n","<table class='nowrap'>\n","<thead><tr>";for($vf=0;$vf<count($I);$vf++){$l=(array)$G->fetch_field();$B=$l["name"];$sh=$l["orgtable"];$rh=$l["orgname"];$H[$l["table"]]=$sh;if($th&&$w=="sql")$Yf[$vf]=($B=="table"?"table=":($B=="possible_keys"?"indexes=":null));elseif($sh!=""){if(!isset($v[$sh])){$v[$sh]=[];foreach(indexes($sh,$f)as$u){if($u["type"]=="PRIMARY"){$v[$sh]=array_flip($u["columns"]);break;}}$d[$sh]=$v[$sh];}if(isset($d[$sh][$rh])){unset($d[$sh][$rh]);$v[$sh][$rh]=$vf;$Yf[$vf]=$sh;}}if($l["charsetnr"]==63)$lb[$vf]=true;$U[$vf]=$l["type"];echo"<th".($sh!=""||$l["name"]!=$rh?" title='".h(($sh!=""?"$sh.":"").$rh)."'":"").">".h($B).($th?doc_link(['sql'=>"explain-output.html#explain_".strtolower($B),'mariadb'=>"explain/#the-columns-in-explain-select",]):"");}echo"</thead>\n";}echo"<tr>";foreach($I
