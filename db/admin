select($H,$f=null,$wh=[],$x=0){$Xf=[];$v=[];$e=[];$eb=[];$vl=[];$I=[];for($r=0;(!$x||$r<$x)&&($J=$H->fetch_row());$r++){if(!$r){echo"<div class='scrollable'>\n","<table class='nowrap'>\n","<thead><tr>";for($vf=0;$vf<count($J);$vf++){$l=(array)$H->fetch_field();$A=$l["name"];$vh=$l["orgtable"];$uh=$l["orgname"];$I[$l["table"]]=$vh;if($wh&&DIALECT=="sql")$Xf[$vf]=($A=="table"?"table=":($A=="possible_keys"?"indexes=":null));elseif($vh!=""){if(!isset($v[$vh])){$v[$vh]=[];foreach(indexes($vh,$f)as$u){if($u["type"]=="PRIMARY"){$v[$vh]=array_flip($u["columns"]);break;}}$e[$vh]=$v[$vh];}if(isset($e[$vh][$uh])){unset($e[$vh][$uh]);$v[$vh][$uh]=$vf;$Xf[$vf]=$vh;}}if($l["charsetnr"]==63)$eb[$vf]=true;$vl[$vf]=$l["type"];echo"<th".($vh!=""||$l["name"]!=$uh?" title='".h(($vh!=""?"$vh.":"").$uh)."'":"").">".h($A).($wh?doc_link(['sql'=>"explain-output.html#explain_".strtolower($A),'mariadb'=>"explain/#the-columns-in-explain-select",]):"");}echo"</thead>\n";}echo"<tr>";foreach($J
as$w=>$X){$y="";if(isset($Xf[$w])&&!$e[$Xf[$w]]){if($wh&&DIALECT=="sql"){$R=$J[array_search("table=",$Xf)];$y=ME.$Xf[$w].urlencode($wh[$R]!=""?$wh[$R]:$R);}else{$y=ME."edit=".urlencode($Xf[$w]);foreach($v[$Xf[$w]]as$Fb=>$vf)$y
.="&where".urlencode("[".bracket_escape($Fb)."]")."=".urlencode($J[$vf]);}}elseif(is_web_url($X))$y=$X;if($X===null)$X="<i>NULL</i>";elseif($eb[$w]&&!is_utf8($X))$X="<i>".lang(24,strlen($X))."</i>";else{$X=h($X);if($vl[$w]==254)$X="<code>$X</code>";}if($y)$X="<a href='".h($y)."'".(is_web_url($y)?target_blank():'').">$X</a>";echo"<td>$X";}}echo($r?"</table>\n</div>":"<p class='message'>".lang(61))."\n";return$I;}function
