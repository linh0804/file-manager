as$li){if(method_exists($li,$Fg))$this->hooks[$Fg][]=$li;}}if(isset(self::$AppendMethods[$Fg])?self::$AppendMethods[$Fg]:false)array_unshift($this->hooks[$Fg],$b);else$this->hooks[$Fg][]=$b;}}function
