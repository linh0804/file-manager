__call($A,array$E){$Ja=isset(self::$AppendMethods[$A])?self::$AppendMethods[$A]:false;$J=$Ja?[]:null;assert(isset($this->hooks[$A]),"Calling unknown plugin method: $A");foreach($this->hooks[$A]as$li){$Y=call_user_func_array([$li,$A],$E);if($Y!==null){if($Ja)$J+=$Y;else
return$Y;}}return$J;}function
