fillForeignDescriptions(array$J,array$ge){return$J;}function
formatSelectionValue($X,$z,$l,$zh){if($X===null)$Jk="<i>NULL</i>";elseif(!$l)$Jk=$X;elseif(preg_match("~char|binary|boolean~",$l["type"])&&!preg_match("~var~",$l["type"]))$Jk="<code>$X</code>";elseif(preg_match('~blob|bytea|raw|file~',$l["type"])&&!is_utf8($X))$Jk="<i>".lang(24,strlen($zh))."</i>";elseif($this->admin->detectJson($l["type"],$zh))$Jk="<code class='jush-js'>$X</code>";else$Jk=$X;if($z)$Jk="<a href='".h($z)."'".(is_web_url($z)?target_blank():"").">$Jk</a>";return$Jk;}function
