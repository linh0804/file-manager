formatMessageQuery($H,$ll,$Nd=false){restart_session();$Qe=&get_session("queries");if(!isset($Qe[$_GET["db"]]))$Qe[$_GET["db"]]=[];if(strlen($H)>1e6)$H=preg_replace('~[\x80-\xFF]+$~','',substr($H,0,1e6))."\n…";$Qe[$_GET["db"]][]=[$H,time(),$ll];$Fk=support("sql");$qm=!$Nd?Driver::get()->warnings():null;$nk="sql-".count($Qe[$_GET["db"]]);$rm="warnings-".count($Qe[$_GET["db"]]);$K=" ";if($qm)$K
