formatSelectionValue($X,$y,$l,$Bh){if($X===null)$Nk="<i>NULL</i>";elseif(!$l)$Nk=$X;elseif(preg_match("~char|binary|boolean~",$l["type"])&&!preg_match("~var~",$l["type"]))$Nk="<code>$X</code>";elseif(preg_match('~blob|bytea|raw|file~',$l["type"])&&!is_utf8($X))$Nk="<i>".lang(24,strlen($Bh))."</i>";elseif($this->admin->detectJson($l["type"],$Bh))$Nk="<code class='jush-js'>$X</code>";else$Nk=$X;if($y)$Nk="<a href='".h($y)."'".(is_web_url($y)?target_blank():"").">$Nk</a>";return$Nk;}function
