alter_table($R,$A,$m,$he,$Ob,$nd,$Fb,$Ra,$Yh){$c=[];$Gi=[];if($R!=""&&$R!=$A)$Gi[]="ALTER TABLE ".table($R)." RENAME TO ".table($A);$Ej="";foreach($m
as$l){$d=idf_escape($l[0]);$X=$l[1];if(!$X)$c[]="DROP $d";else{$bm=$X[5];unset($X[5]);if($l[0]==""){if(isset($X[6]))$X[1]=($X[1]==" bigint"?" big":($X[1]==" smallint"?" small":" "))."serial";$c[]=($R!=""?"ADD ":"  ").implode($X);if(isset($X[6]))$c[]=($R!=""?"ADD":" ")." PRIMARY KEY ($X[0])";}else{if($d!=$X[0])$Gi[]="ALTER TABLE ".table($A)." RENAME $d TO $X[0]";$c[]="ALTER $d TYPE$X[1]";$Fj=$R."_".idf_unescape($X[0])."_seq";$c[]="ALTER $d ".($X[3]?"SET".preg_replace('~GENERATED ALWAYS(.*) STORED~','EXPRESSION\1',$X[3]):(isset($X[6])?"SET DEFAULT nextval(".q($Fj).")":"DROP DEFAULT"));if(isset($X[6]))$Ej="CREATE SEQUENCE IF NOT EXISTS ".idf_escape($Fj)." OWNED BY ".idf_escape($R).".$X[0]";$c[]="ALTER $d ".($X[2]==" NULL"?"DROP NOT":"SET").$X[2];}if($l[0]!=""||$bm!="")$Gi[]="COMMENT ON COLUMN ".table($A).".$X[0] IS ".($bm!=""?substr($bm,9):"''");}}$c=array_merge($c,$he);if($R=="")array_unshift($Gi,"CREATE TABLE ".table($A)." (\n".implode(",\n",$c)."\n)");elseif($c)array_unshift($Gi,"ALTER TABLE ".table($R)."\n".implode(",\n",$c));if($Ej)array_unshift($Gi,$Ej);if($Ob!==null)$Gi[]="COMMENT ON TABLE ".table($A)." IS ".q($Ob);if($Ra!="");foreach($Gi
as$H){if(!queries($H))return
false;}return
true;}function
alter_indexes($R,$c){$lc=[];$Uc=[];$Gi=[];foreach($c
as$X){if($X[0]!="INDEX")$lc[]=($X[2]=="DROP"?"\nDROP CONSTRAINT ".idf_escape($X[1]):"\nADD".($X[1]!=""?" CONSTRAINT ".idf_escape($X[1]):"")." $X[0] ".($X[0]=="PRIMARY"?"KEY ":"")."(".implode(", ",$X[2]).")");elseif($X[2]=="DROP")$Uc[]=idf_escape($X[1]);else$Gi[]="CREATE INDEX ".idf_escape($X[1]!=""?$X[1]:uniqid($R."_"))." ON ".table($R)." (".implode(", ",$X[2]).")";}if($lc)array_unshift($Gi,"ALTER TABLE ".table($R).implode(",",$lc));if($Uc)array_unshift($Gi,"DROP INDEX ".implode(", ",$Uc));foreach($Gi
as$H){if(!queries($H))return
false;}return
true;}function
