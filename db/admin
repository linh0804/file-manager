table_status($_="",$Td=false){$Hk=Connection::get()->rootQuery('_stats');$Ea=Connection::get()->rootQuery('_aliases');if(empty($Hk)||empty($Ea))return[];$I=[];if($_!=""){if(isset($Hk["indices"][$_]))return
