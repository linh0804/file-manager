formatSelectQuery($F,$ck,$Md=false){global$w,$j;$nk=support("sql");$Tl=!$Md?$j->warnings():null;if($nk)$F
.=";";$pk=$w=="elastic"?"js":$w;$H="<pre><code class='jush-$pk'>".h(str_replace("\n"," ",$F))."</code></pre>\n";$H
