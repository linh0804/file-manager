create_sql($R,$Ua,$jk){if(is_view(table_status($R))){$Sl=view($R);return"CREATE VIEW ".table($R)." AS $Sl[select]";}$m=array();$E=false;foreach(fields($R)as$A=>$l){$X=process_field($l,$l);if($X[6])$E=true;$m[]=implode("",$X);}foreach(indexes($R)as$A=>$u){if(!$E||$u["type"]!="PRIMARY"){$e=array();foreach($u["columns"]as$w=>$X)$e[]=idf_escape($X).($u["descs"][$w]?" DESC":"");$A=idf_escape($A);$m[]=($u["type"]=="INDEX"?"INDEX $A":"CONSTRAINT $A ".($u["type"]=="UNIQUE"?"UNIQUE":"PRIMARY KEY"))." (".implode(", ",$e).")";}}foreach(Driver::get()->checkConstraints($R)as$A=>$tb)$m[]="CONSTRAINT ".idf_escape($A)." CHECK ($tb)";return"CREATE TABLE ".table($R)." (\n\t".implode(",\n\t",$m)."\n)";}function
foreign_keys_sql($R){$m=array();foreach(foreign_keys($R)as$be)$m[]=ltrim(format_foreign_key($be));return($m?"ALTER TABLE ".table($R)." ADD\n\t".implode(",\n\t",$m).";\n\n":"");}function
