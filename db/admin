alter_table($R,$A,$m,$je,$Pb,$pd,$Gb,$Sa,$Zh){$c=[];$Hi=[];if($R!=""&&$R!=$A)$Hi[]="ALTER TABLE ".table($R)." RENAME TO ".table($A);$Hj="";foreach($m
as$l){$d=idf_escape($l[0]);$X=$l[1];if(!$X)$c[]="DROP $d";else{$em=$X[5];unset($X[5]);if($l[0]==""){if(isset($X[6]))$X[1]=($X[1]==" bigint"?" big":($X[1]==" smallint"?" small":" "))."serial";$c[]=($R!=""?"ADD ":"  ").implode($X);if(isset($X[6]))$c[]=($R!=""?"ADD":" ")." PRIMARY KEY ($X[0])";}else{if($d!=$X[0])$Hi[]="ALTER TABLE ".table($A)." RENAME $d TO $X[0]";$c[]="ALTER $d TYPE$X[1]";$Ij=$R."_".idf_unescape($X[0])."_seq";$c[]="ALTER $d ".($X[3]?"SET".preg_replace('~GENERATED ALWAYS(.*) STORED~','EXPRESSION\1',$X[3]):(isset($X[6])?"SET DEFAULT nextval(".q($Ij).")":"DROP DEFAULT"));if(isset($X[6]))$Hj="CREATE SEQUENCE IF NOT EXISTS ".idf_escape($Ij)." OWNED BY ".idf_escape($R).".$X[0]";$c[]="ALTER $d ".($X[2]==" NULL"?"DROP NOT":"SET").$X[2];}if($l[0]!=""||$em!="")$Hi[]="COMMENT ON COLUMN ".table($A).".$X[0] IS ".($em!=""?substr($em,9):"''");}}$c=array_merge($c,$je);if($R=="")array_unshift($Hi,"CREATE TABLE ".table($A)." (\n".implode(",\n",$c)."\n)");elseif($c)array_unshift($Hi,"ALTER TABLE ".table($R)."\n".implode(",\n",$c));if($Hj)array_unshift($Hi,$Hj);if($Pb!==null)$Hi[]="COMMENT ON TABLE ".table($A)." IS ".q($Pb);if($Sa!="");foreach($Hi
as$H){if(!queries($H))return
false;}return
true;}function
