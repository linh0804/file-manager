select(Result$J,$f=null,$Eh=[],$x=0){$eg=[];$v=[];$e=[];$db=[];$Kl=[];$K=[];for($r=0;(!$x||$r<$x)&&($L=$J->fetchRow());$r++){if(!$r){echo"<div class='scrollable'>\n","<table class='nowrap'>\n","<thead><tr>";for($Cf=0;$Cf<count($L);$Cf++){$l=$J->fetchField();if(!$l){echo"<th></th>";continue;}$A=$l->name;$Dh=isset($l->orgtable)?$l->orgtable:"";$Ch=isset($l->orgname)?$l->orgname:$A;if(isset($l->table))$K[$l->table]=$Dh;if($Eh&&DIALECT=="sql")$eg[$Cf]=($A=="table"?"table=":($A=="possible_keys"?"indexes=":null));elseif($Dh!=""){if(!isset($v[$Dh])){$v[$Dh]=[];foreach(indexes($Dh,$f)as$u){if($u["type"]=="PRIMARY"){$v[$Dh]=array_flip($u["columns"]);break;}}$e[$Dh]=$v[$Dh];}if(isset($e[$Dh][$Ch])){unset($e[$Dh][$Ch]);$v[$Dh][$Ch]=$Cf;$eg[$Cf]=$Dh;}}if($l->charsetnr==63)$db[$Cf]=true;$Kl[$Cf]=$l->type;echo"<th".($Dh!=""||$l->name!=$Ch?" title='".h(($Dh!=""?"$Dh.":"").$Ch)."'":"").">".h($A).($Eh?doc_link(['sql'=>"explain-output.html#explain_".strtolower($A),'mariadb'=>"explain/#the-columns-in-explain-select",]):"");}echo"</thead>\n";}echo"<tr>";foreach($L
as$w=>$X){$y="";if(isset($eg[$w])&&!$e[$eg[$w]]){if($Eh&&DIALECT=="sql"){$R=$L[array_search("table=",$eg)];$y=ME.$eg[$w].urlencode($Eh[$R]!=""?$Eh[$R]:$R);}else{$y=ME."edit=".urlencode($eg[$w]);foreach($v[$eg[$w]]as$Eb=>$Cf)$y
.="&where".urlencode("[".bracket_escape($Eb)."]")."=".urlencode($L[$Cf]);}}elseif(is_web_url($X))$y=$X;if($X===null)$X="<i>NULL</i>";elseif($db[$w]&&!is_utf8($X))$X="<i>".lang(37,strlen($X))."</i>";else{$X=h($X);if($Kl[$w]==254)$X="<code>$X</code>";}if($y)$X="<a href='".h($y)."'".(is_web_url($y)?target_blank():'').">$X</a>";$Bb=$Kl[$w]<=9||$Kl[$w]==246?"class='number'":"";echo"<td $Bb>$X</td>";}}echo($r?"</table>\n</div>":"<p class='message'>".lang(81))."\n";return$K;}function
referencable_primary($Ej){$K=[];foreach(table_status('',true)as$Qk=>$R){if($Qk!=$Ej&&fk_support($R)){foreach(fields($Qk)as$l){if($l["primary"]){if($K[$Qk]){unset($K[$Qk]);break;}$K[$Qk]=$l;}}}}return$K;}function
