parameters:
    phpVersion: 80200
    level: 2
    paths:
        - .
    excludePaths:
        - node_modules/
        - vendor/
        - css/
        - db/
        - tmp/
        - icon/
        - js/
        - json/
    ignoreErrors:
        -
            identifier: variable.undefined
        -
            identifier: constant.notFound
