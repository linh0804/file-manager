foreign_keys_sql($R){$K="";$tk=table_status($R);$ce=foreign_keys($R);ksort($ce);foreach($ce
as$be=>$ae)$K
.="ALTER TABLE ONLY ".idf_escape($tk['nspname']).".".idf_escape($tk['Name'])." ADD CONSTRAINT ".idf_escape($be)." $ae[definition] ".($ae['deferrable']?'DEFERRABLE':'NOT DEFERRABLE').";\n";return($K?"$K\n":$K);}function
create_sql($R,$Ra,$wk){$fj=[];$Gj=[];$tk=table_status($R);if(is_view($tk)){$hm=view($R);return
rtrim("CREATE VIEW ".idf_escape($R)." AS $hm[select]",";");}$m=fields($R);if(!$tk||empty($m))return
false;$K="CREATE TABLE ".idf_escape($tk['nspname']).".".idf_escape($tk['Name'])." (\n    ";foreach($m
as$l){$Uh=idf_escape($l['field']).' '.$l['full_type'].default_value($l).($l['attnotnull']?" NOT NULL":"");$fj[]=$Uh;if(preg_match('~nextval\(\'([^\']+)\'\)~',$l['default'],$_)){$Fj=$_[1];$M=get_rows((Connection::get()->isMinVersion("10")?"SELECT *, cache_size AS cache_value FROM pg_sequences WHERE schemaname = current_schema() AND sequencename = ".q(idf_unescape($Fj)):"SELECT * FROM $Fj"),null,"-- ");$ik=reset($M);$Gj[]=($wk=="DROP+CREATE"?"DROP SEQUENCE IF EXISTS $Fj;\n":"")."CREATE SEQUENCE $Fj INCREMENT $ik[increment_by] MINVALUE $ik[min_value] MAXVALUE $ik[max_value]".($Ra&&$ik['last_value']?" START ".($ik["last_value"]+1):"")." CACHE $ik[cache_value];";}}if(!empty($Gj))$K=implode("\n\n",$Gj)."\n\n$K";$G="";foreach(indexes($R)as$ff=>$u){if($u['type']=='PRIMARY'){$G=$ff;$fj[]="CONSTRAINT ".idf_escape($ff)." PRIMARY KEY (".implode(', ',array_map('AdminNeo\idf_escape',$u['columns'])).")";}}foreach(Driver::get()->checkConstraints($R)as$Yb=>$dc)$fj[]="CONSTRAINT ".idf_escape($Yb)." CHECK $dc";$K
.=implode(",\n    ",$fj)."\n) WITH (oids = ".($tk['Oid']?'true':'false').");";if($tk['Comment'])$K
.="\n\nCOMMENT ON TABLE ".idf_escape($tk['nspname']).".".idf_escape($tk['Name'])." IS ".q($tk['Comment']).";";foreach($m
as$Rd=>$l){if($l['comment'])$K
.="\n\nCOMMENT ON COLUMN ".idf_escape($tk['nspname']).".".idf_escape($tk['Name']).".".idf_escape($Rd)." IS ".q($l['comment']).";";}foreach(get_rows("SELECT indexdef FROM pg_catalog.pg_indexes WHERE schemaname = current_schema() AND tablename = ".q($R).($G?" AND indexname != ".q($G):""),null,"-- ")as$L)$K
.="\n\n$L[indexdef];";return
rtrim($K,';');}function
