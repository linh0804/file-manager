int32($Pg){while($Pg>=2147483648)$Pg-=4294967296;while($Pg<=-2147483649)$Pg+=4294967296;return(int)$Pg;}function
long2str($W,$mm){$oj='';foreach($W
as$X)$oj
.=pack('V',$X);return$mm?substr($oj,0,end($W)):$oj;}function
str2long($oj,$mm){$W=array_values(unpack('V*',str_pad($oj,4*ceil(strlen($oj)/4),"\0")));if($mm)$W[]=strlen($oj);return$W;}function
xxtea_mx($Am,$_m,$Ak,$Df){return
int32((($Am>>5&0x7FFFFFF)^$_m<<2)+(($_m>>3&0x1FFFFFFF)^$Am<<4))^int32(($Ak^$_m)+($Df^$Am));}function
xxtea_encrypt_string($ki,$w){$w=array_values(unpack("V*",pack("H*",md5($w))));$W=str2long($ki,true);$Pg=count($W)-1;$Am=$W[$Pg];$_m=$W[0];$Fi=floor(6+52/($Pg+1));$Ak=0;while($Fi-->0){$Ak=int32($Ak+0x9E3779B9);$bd=$Ak>>2&3;for($Ph=0;$Ph<$Pg;$Ph++){$_m=$W[$Ph+1];$Ng=xxtea_mx($Am,$_m,$Ak,$w[$Ph&3^$bd]);$Am=int32($W[$Ph]+$Ng);$W[$Ph]=$Am;}$_m=$W[0];$Ng=xxtea_mx($Am,$_m,$Ak,$w[$Ph&3^$bd]);$Am=int32($W[$Pg]+$Ng);$W[$Pg]=$Am;}return
long2str($W,false);}function
xxtea_decrypt_string($g,$w){$w=array_values(unpack("V*",pack("H*",md5($w))));$W=str2long($g,false);$Pg=count($W)-1;$Am=$W[$Pg];$_m=$W[0];$Fi=floor(6+52/($Pg+1));$Ak=int32($Fi*0x9E3779B9);while($Ak){$bd=$Ak>>2&3;for($Ph=$Pg;$Ph>0;$Ph--){$Am=$W[$Ph-1];$Ng=xxtea_mx($Am,$_m,$Ak,$w[$Ph&3^$bd]);$_m=int32($W[$Ph]-$Ng);$W[$Ph]=$_m;}$Am=$W[$Pg];$Ng=xxtea_mx($Am,$_m,$Ak,$w[$Ph&3^$bd]);$_m=int32($W[0]-$Ng);$W[0]=$_m;$Ak=int32($Ak-0x9E3779B9);}return
long2str($W,true);}const
ENCRYPTION_GCM='aes-256-gcm';const
ENCRYPTION_CBC='aes-256-cbc';const
ENCRYPTION_TAG_LENGTH=16;const
ENCRYPTION_HMAC_LENGTH=64;function
generate_iv($Xf){if(function_exists('random_bytes')){try{return
random_bytes($Xf);}catch(Exception$bd){}}return
openssl_random_pseudo_bytes($Xf);}function
hash_key($w){return
substr(hash('sha512',$w,true),0,32);}function
aes_encrypt_string($ki,$w){$ii=version_compare(PHP_VERSION,"7.1.0")>=0;$Fg=$ii?ENCRYPTION_GCM:ENCRYPTION_CBC;$w=hash_key($w);$zf=generate_iv(openssl_cipher_iv_length($Fg)?:16);if($ii)$_b=openssl_encrypt($ki,$Fg,$w,OPENSSL_RAW_DATA,$zf,$Vk,"",ENCRYPTION_TAG_LENGTH);else{$_b=openssl_encrypt($ki,$Fg,$w,OPENSSL_RAW_DATA,$zf);$Vk=hash_hmac("sha512",$zf.$_b,$w,true);}if($_b===false)return
false;return$zf.$Vk.$_b;}function
aes_decrypt_string($g,$w){$ii=version_compare(PHP_VERSION,"7.1.0")>=0;$Fg=$ii?ENCRYPTION_GCM:ENCRYPTION_CBC;$_f=openssl_cipher_iv_length($Fg)?:16;$Wk=$ii?ENCRYPTION_TAG_LENGTH:ENCRYPTION_HMAC_LENGTH;if(strlen($g)<$_f+$Wk)return
false;$w=hash_key($w);$zf=substr($g,0,$_f);$Vk=substr($g,$_f,$Wk);$_b=substr($g,$_f+$Wk);if($zf===false||$Vk===false||$_b===false)return
false;if($ii)return
openssl_decrypt($_b,$Fg,$w,OPENSSL_RAW_DATA,$zf,$Vk);else{$Re=hash_hmac('sha512',$zf.$_b,$w,true);if(!hash_equals($Vk,$Re))return
false;return
openssl_decrypt($_b,$Fg,$w,OPENSSL_RAW_DATA,$zf);}}function
encrypt_string($ki,$w){if($ki=="")return"";if(extension_loaded('openssl'))return
aes_encrypt_string($ki,$w);else
return
xxtea_encrypt_string($ki,$w);}function
decrypt_string($g,$w){if($g=="")return"";if(extension_loaded('openssl'))return
