detectJson($Rd,&$Y,$mi=null){if(is_array($Y)){$ce=JSON_UNESCAPED_UNICODE|JSON_UNESCAPED_SLASHES|($this->config->isJsonValuesAutoFormat()?JSON_PRETTY_PRINT:0);$Y=json_encode($Y,$ce);return
true;}$ce=JSON_UNESCAPED_UNICODE|JSON_UNESCAPED_SLASHES|($mi?JSON_PRETTY_PRINT:0);if(str_contains($Rd,"json")){if($mi!==null&&$this->config->isJsonValuesAutoFormat())$Y=json_encode(json_decode($Y),$ce);return
true;}if(!$this->config->isJsonValuesDetection())return
false;if($Y!=""&&preg_match('~varchar|text|character varying|String|keyword~',$Rd)&&($Y[0]=="{"||$Y[0]=="[")&&($wf=json_decode($Y))){if($mi!==null&&$this->config->isJsonValuesAutoFormat())$Y=json_encode($wf,$ce);return
true;}return
false;}abstract
