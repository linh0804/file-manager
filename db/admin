open($O,$V,$F,$xc=null,$qi=null,$fk=null){mysqli_report(MYSQLI_REPORT_OFF);list($Ve,$qi)=explode(":",$O,2);$w=Admin::get()->getConfig()->getSslKey();$ob=Admin::get()->getConfig()->getSslCertificate();$mb=Admin::get()->getConfig()->getSslCaCertificate();$rk=$w||$ob||$mb;if($rk){$this->mysqli->ssl_set($w,$ob,$mb,null,null);$fe=Admin::get()->getConfig()->getSslTrustServerCertificate()?64:MYSQLI_CLIENT_SSL;}else$fe=0;$ac=@$this->mysqli->real_connect(($O!=""?$Ve:ini_get("mysqli.default_host")),($O.$V!=""?$V:ini_get("mysqli.default_user")),($O.$V.$F!=""?$F:ini_get("mysqli.default_pw")),$xc,(is_numeric($qi)?$qi:ini_get("mysqli.default_port")),(!is_numeric($qi)?$qi:$fk),$fe);$this->mysqli->options(MYSQLI_OPT_LOCAL_INFILE,false);if($ac){$lf=$this->mysqli->get_server_info();$this->version=str_replace("-MariaDB","",$lf);$this->flavor=str_contains($lf,"MariaDB")?"mariadb":null;}return$ac;}function
