false;}elseif($J->numColumns())return
new
SqLiteResult($J);$this->affectedRows=$this->sqlite->changes();return
true;}function
quote($Q){if(is_utf8($Q))return"'".$this->sqlite->escapeString($Q)."'";else{$g=unpack('H*',$Q);return"x'".reset($g)."'";}}}class
