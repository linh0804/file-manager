static();if(!$Xb&&file_exists("adminneo-config.php")){$Xb=include_once("adminneo-config.php");if(!is_array($Xb)){$Xb=[];$cg="href=https://github.com/adminneo-org/adminneo#configuration ".target_blank();$b->addError(lang(0,"<b>adminneo-config.php</b>")." <a $cg>".lang(1)."</a>");}}$Xb=new
