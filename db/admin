fields($R){$K=[];$G="";foreach(get_rows("PRAGMA table_".(Connection::get()->isMinVersion("3.31")?"x":"")."info(".table($R).")")as$L){$A=$L["name"];$U=strtolower($L["type"]);$j=$L["dflt_value"];$K[$A]=["field"=>$A,"type"=>(preg_match('~int~i',$U)?"integer":(preg_match('~char|clob|text~i',$U)?"text":(preg_match('~blob~i',$U)?"blob":(preg_match('~real|floa|doub~i',$U)?"real":"numeric")))),"full_type"=>$U,"default"=>(preg_match("~^'(.*)'$~",$j,$z)?str_replace("''","'",$z[1]):($j=="NULL"?null:$j)),"null"=>!$L["notnull"],"privileges"=>["select"=>1,"insert"=>1,"update"=>1,"where"=>1,"order"=>1],"primary"=>$L["pk"],];if($L["pk"]){if($G!="")$K[$G]["auto_increment"]=false;elseif(preg_match('~^integer$~i',$U))$K[$A]["auto_increment"]=true;$G=$A;}}$mk=Connection::get()->getValue("SELECT sql FROM sqlite_master WHERE type = 'table' AND name = ".q($R));$t='(("[^"]*+")+|[a-z0-9_]+)';preg_match_all('~'.$t.'\s+text\s+COLLATE\s+(\'[^\']+\'|\S+)~i',$mk,$_,PREG_SET_ORDER);foreach($_
as$z){$A=str_replace('""','"',preg_replace('~^"|"$~','',$z[1]));if($K[$A])$K[$A]["collation"]=trim($z[3],"'");}preg_match_all('~'.$t.'\s.*GENERATED ALWAYS AS \((.+)\) (STORED|VIRTUAL)~i',$mk,$_,PREG_SET_ORDER);foreach($_
as$z){$A=str_replace('""','"',preg_replace('~^"|"$~','',$z[1]));$K[$A]["default"]=$z[3];$K[$A]["generated"]=strtoupper($z[4]);}return$K;}function
indexes($R,$f=null){if(!is_object($f))$f=Connection::get();$K=[];$mk=$f->getValue("SELECT sql FROM sqlite_master WHERE type = 'table' AND name = ".q($R));if(preg_match('~\bPRIMARY\s+KEY\s*\((([^)"]+|"[^"]*"|`[^`]*`)++)~i',$mk,$z)){$K[""]=["type"=>"PRIMARY","columns"=>[],"lengths"=>[],"descs"=>[]];preg_match_all('~((("[^"]*+")+|(?:`[^`]*+`)+)|(\S+))(\s+(ASC|DESC))?(,\s*|$)~i',$z[1],$_,PREG_SET_ORDER);foreach($_
as$z){$K[""]["columns"][]=idf_unescape($z[2]).$z[4];$K[""]["descs"][]=(preg_match('~DESC~i',$z[5])?'1':null);}}if(!$K){foreach(fields($R)as$A=>$l){if($l["primary"])$K[""]=["type"=>"PRIMARY","columns"=>[$A],"lengths"=>[],"descs"=>[null]];}}$qk=get_key_vals("SELECT name, sql FROM sqlite_master WHERE type = 'index' AND tbl_name = ".q($R),$f);foreach(get_rows("PRAGMA index_list(".table($R).")",$f)as$L){$A=$L["name"];$u=["type"=>($L["unique"]?"UNIQUE":"INDEX")];$u["lengths"]=[];$u["descs"]=[];foreach(get_rows("PRAGMA index_info(".idf_escape($A).")",$f)as$oj){$u["columns"][]=$oj["name"];$u["descs"][]=null;}if(preg_match('~^CREATE( UNIQUE)? INDEX '.preg_quote(idf_escape($A).' ON '.idf_escape($R),'~').' \((.*)\)$~i',$qk[$A],$Xi)){preg_match_all('/("[^"]*+")+( DESC)?/',$Xi[2],$_);foreach($_[2]as$w=>$X){if($X)$u["descs"][$w]='1';}}if(!$K[""]||$u["type"]!="UNIQUE"||$u["columns"]!=$K[""]["columns"]||$u["descs"]!=$K[""]["descs"]||!preg_match("~^sqlite_~",$A))$K[$A]=$u;}return$K;}function
foreign_keys($R){$K=[];foreach(get_rows("PRAGMA foreign_key_list(".table($R).")")as$L){$p=&$K[$L["id"]];if(!$p)$p=$L;$p["source"][]=$L["from"];$p["target"][]=$L["to"];}return$K;}function
