trigger($A,$R){if($A=="")return["Statement"=>"EXECUTE PROCEDURE ()"];$e=[];$Z="WHERE trigger_schema = current_schema() AND event_object_table = ".q($R)." AND trigger_name = ".q($A);foreach(get_rows("SELECT * FROM information_schema.triggered_update_columns $Z")as$L)$e[]=$L["event_object_column"];$K=[];foreach(get_rows('SELECT trigger_name AS "Trigger", action_timing AS "Timing", event_manipulation AS "Event", \'FOR EACH \' || action_orientation AS "Type", action_statement AS "Statement" FROM information_schema.triggers '."$Z ORDER BY event_manipulation DESC")as$L){if($e&&$L["Event"]=="UPDATE")$L["Event"].=" OF";$L["Of"]=implode(", ",$e);if($K)$L["Event"].=" OR $K[Event]";$K=$L;}return$K;}function
