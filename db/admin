false;}$H=@pg_query($this->connection,$F);$this->error="";if(!$H){$this->error=pg_last_error($this->connection);$I=false;}elseif(!pg_num_fields($H)){$this->affected_rows=pg_affected_rows($H);$I=true;}else$I=new
