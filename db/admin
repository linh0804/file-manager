null;if($ng)return"SET STATEMENT max_statement_time=$ml FOR $H";elseif(preg_match('~^(SELECT\b)(.+)~is',$H,$z))return"$z[1] /*+ MAX_EXECUTION_TIME(".($ml*1000).") */ $z[2]";else
return
null;}function
