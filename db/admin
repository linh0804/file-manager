detectJson($Sd,&$Y,$xi=null){if(is_array($Y)){$fe=JSON_UNESCAPED_UNICODE|JSON_UNESCAPED_SLASHES|($this->config->isJsonValuesAutoFormat()?JSON_PRETTY_PRINT:0);$Y=json_encode($Y,$fe);return
true;}$fe=JSON_UNESCAPED_UNICODE|JSON_UNESCAPED_SLASHES|($xi?JSON_PRETTY_PRINT:0);if(str_contains($Sd,"json")){if($xi!==null&&$this->config->isJsonValuesAutoFormat())$Y=json_encode(json_decode($Y),$fe);return
true;}if(!$this->config->isJsonValuesDetection())return
false;if($Y!=""&&preg_match('~varchar|text|character varying|String|keyword~',$Sd)&&($Y[0]=="{"||$Y[0]=="[")&&($Df=json_decode($Y))){if($xi!==null&&$this->config->isJsonValuesAutoFormat())$Y=json_encode($Df,$fe);return
true;}return
false;}abstract
