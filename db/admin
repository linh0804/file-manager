filterListWithPattern(array$vm,$si,$Qf){$I=[];foreach($vm
as$u=>$Y){if(is_array($Y)){if($Ok=$this->filterListWithPattern($Y,$si,$Qf))$I[$u]=$Ok;}elseif(($Qf&&preg_match($si,$Y))||(!$Qf&&!preg_match($si,$Y)))$I[$u]=$Y;}return$I;}abstract
