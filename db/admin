alter_table($R,$_,$m,$ke,$Pb,$qd,$Gb,$Sa,$di){$c=[];$Rb=[];$Lh=fields($R);foreach($m
as$l){$d=idf_escape($l[0]);$X=$l[1];if(!$X)$c["DROP"][]=" COLUMN $d";else{$X[1]=preg_replace("~( COLLATE )'(\\w+)'~",'\1\2',$X[1]);$Rb[$l[0]]=$X[5];unset($X[5]);if(preg_match('~ AS ~',$X[3]))unset($X[1],$X[2]);if($l[0]=="")$c["ADD"][]="\n  ".implode("",$X).($R==""?substr($ke[$X[0]],16+strlen($X[0])):"");else{$j=$X[3];unset($X[3]);unset($X[6]);if($d!=$X[0])queries("EXEC sp_rename ".q(table($R).".$d").", ".q(idf_unescape($X[0])).", 'COLUMN'");$c["ALTER COLUMN ".implode("",$X)][]="";$Kh=$Lh[$l[0]];if(default_value($Kh)!=$j){if($Kh["default"]!==null)$c["DROP"][]=" ".idf_escape($Kh["default_constraint"]);if($j)$c["ADD"][]="\n $j FOR $d";}}}}if($R=="")return
