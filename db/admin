json_row($w,$X=null){static$ae=true;if($ae)echo"{";if($w!=""){echo($ae?"":",")."\n\t\"".addcslashes($w,"\r\n\t\"\\/").'": '.($X!==null?'"'.addcslashes($X,"\r\n\t\"\\/").'"':'null');$ae=false;}else{echo"\n}\n";$ae=true;}}function
