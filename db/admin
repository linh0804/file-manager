as$I){$Y="(".implode(", ",$I).")";if($fm&&(strlen($vi)+$Yf+strlen($Y)+strlen($Ck)>1e6)){if(!queries($vi.implode(",\n",$fm).$Ck))return
false;$fm=[];$Yf=0;}$fm[]=$Y;$Yf+=strlen($Y)+2;}return
queries($vi.implode(",\n",$fm).$Ck);}function
