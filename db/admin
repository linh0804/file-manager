function
filterListWithWildcards(array$fm,array$Zd,$If,array$Jk=[]){if(!$fm||!$Zd)return$fm;$u=array_search("__system",$Zd);if($u!==false){unset($Zd[$u]);$Zd=array_merge($Zd,$Jk);}array_walk($Zd,function(&$Y){$Y=str_replace('\\*',".*",preg_quote($Y,"~"));});$hi='~^('.implode("|",$Zd).')$~';return$this->filterListWithPattern($fm,$hi,$If);}private
function
filterListWithPattern(array$fm,$hi,$If){$J=[];foreach($fm
as$w=>$Y){if(is_array($Y)){if($_k=$this->filterListWithPattern($Y,$hi,$If))$J[$w]=$_k;}elseif(($If&&preg_match($hi,$Y))||(!$If&&!preg_match($hi,$Y)))$J[$w]=$Y;}return$J;}abstract
