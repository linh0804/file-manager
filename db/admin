alter_table($R,$A,$m,$he,$Ob,$nd,$Fb,$Ra,$Yh){$c=[];$Qb=[];$Gh=fields($R);foreach($m
as$l){$d=idf_escape($l[0]);$X=$l[1];if(!$X)$c["DROP"][]=" COLUMN $d";else{$X[1]=preg_replace("~( COLLATE )'(\\w+)'~",'\1\2',$X[1]);$Qb[$l[0]]=$X[5];unset($X[5]);if(preg_match('~ AS ~',$X[3]))unset($X[1],$X[2]);if($l[0]=="")$c["ADD"][]="\n  ".implode("",$X).($R==""?substr($he[$X[0]],16+strlen($X[0])):"");else{$j=$X[3];unset($X[3]);unset($X[6]);if($d!=$X[0])queries("EXEC sp_rename ".q(table($R).".$d").", ".q(idf_unescape($X[0])).", 'COLUMN'");$c["ALTER COLUMN ".implode("",$X)][]="";$Fh=$Gh[$l[0]];if(default_value($Fh)!=$j){if($Fh["default"]!==null)$c["DROP"][]=" ".idf_escape($Fh["default_constraint"]);if($j)$c["ADD"][]="\n $j FOR $d";}}}}if($R=="")return
