printTableStructure(array$m){echo"<div class='scrollable'>\n","<table class='nowrap'>\n","<thead><tr><th>",lang(25),"</th><td>",lang(26),"</td><td>",lang(27),"</td>",(support("comment")?"<td>".lang(28)."</td>":""),"</thead>\n";$Kl=Driver::get()->getUserTypes();foreach($m
as$l){echo"<tr>","<th>",h($l["field"]),"</th>","<td>";$U=h($l["full_type"]);if(in_array($U,$Kl))echo"<a href='".h(ME.'type='.urlencode($U))."'>$U</a>";else
echo$U;if($l["null"])echo" <i>NULL</i>";if($l["auto_increment"])echo" <i>".lang(29)."</i>";$j=h($l["default"]);if(isset($l["default"]))echo" <span title='".lang(30)."'>[<b>",$l["generated"]?"<code class='jush-".DIALECT."'>$j</code>":$j,"</b>]</span>";echo"</td>","<td>",h($l["collation"]),"</td>";if(support("comment"))echo"<td>",$this->admin->formatComment($l["comment"]),"</td>";echo"\n";}echo"</table>\n","</div>\n";}function
printTablePartitions(array$Nh){$Kj=$Nh["partition_by"]=="RANGE"||$Nh["partition_by"]=="LIST";echo"<p>","<code>{$Nh["partition_by"]} ({$Nh["partition"]})</code>";if(!$Kj)echo" ".lang(31).": ".h($Nh["partitions"]);echo"</p>";if($Kj){echo"<table>\n","<thead><tr><th>".lang(32)."</th><td>".lang(33)."</td></tr></thead>\n";foreach($Nh["partition_names"]as$w=>$A)echo"<tr><th>".h($A)."</th><td>".h($Nh["partition_values"][$w])."\n";echo"</table>\n";}}function
printTableIndexes(array$v){echo"<table>\n","<thead><tr><th>".lang(26)."</th><td>".lang(25)." (".lang(34).")</td></tr></thead>\n";foreach($v
