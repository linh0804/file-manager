function
filterListWithWildcards(array$cm,array$Xd,$Gf,array$Gk=[]){if(!$cm||!$Xd)return$cm;$u=array_search("__system",$Xd);if($u!==false){unset($Xd[$u]);$Xd=array_merge($Xd,$Gk);}array_walk($Xd,function(&$Y){$Y=str_replace('\\*',".*",preg_quote($Y,"~"));});$fi='~^('.implode("|",$Xd).')$~';return$this->filterListWithPattern($cm,$fi,$Gf);}private
function
filterListWithPattern(array$cm,$fi,$Gf){$J=[];foreach($cm
as$w=>$Y){if(is_array($Y)){if($xk=$this->filterListWithPattern($Y,$fi,$Gf))$J[$w]=$xk;}elseif(($Gf&&preg_match($fi,$Y))||(!$Gf&&!preg_match($fi,$Y)))$J[$w]=$Y;}return$J;}abstract
