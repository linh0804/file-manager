formatSelectionValue($X,$y,$l,$Kh){if($X===null)$el="<i>NULL</i>";elseif(!$l)$el=$X;elseif(preg_match("~char|binary|boolean~",$l["type"])&&!preg_match("~var~",$l["type"]))$el="<code>$X</code>";elseif(preg_match('~blob|bytea|raw|file~',$l["type"])&&!is_utf8($X))$el="<i>".lang(37,strlen($Kh))."</i>";elseif($this->admin->detectJson($l["type"],$Kh))$el="<code class='jush-js'>$X</code>";else$el=$X;if($y)$el="<a href='".h($y)."'".(is_web_url($y)?target_blank():"").">$el</a>";return$el;}function
