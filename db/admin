.="&where".urlencode("[".bracket_escape($Mb)."]")."=".urlencode($I[$vf]);}}elseif(is_web_url($X))$z=$X;if($X===null)$X="<i>NULL</i>";elseif($lb[$x]&&!is_utf8($X))$X="<i>".lang(24,strlen($X))."</i>";else{$X=h($X);if($U[$x]==254)$X="<code>$X</code>";}if($z)$X="<a href='".h($z)."'".(is_web_url($z)?target_blank():'').">$X</a>";echo"<td>$X";}}echo($r?"</table>\n</div>":"<p class='message'>".lang(62))."\n";return$H;}function
