as$Bd){list($Fb,$kh,$X)=explode(" ",$Bd,3);if($Fb=="_id"&&preg_match('~^(MongoDB\\\\BSON\\\\ObjectID)\("(.+)"\)$~',$X,$z)){list(,$Cb,$X)=$z;$X=new$Cb($X);}if(!in_array($kh,Admin::get()->getOperators()))continue;if(preg_match('~^\(f\)(.+)~',$kh,$z)){$X=(float)$X;$kh=$z[1];}elseif(preg_match('~^\(date\)(.+)~',$kh,$z)){$vc=new
