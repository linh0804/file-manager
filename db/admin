Pluginer{private
static$InternalMethods=["inject"=>true,"getConfig"=>true,];private
static$AppendMethods=["getErrors"=>true,"getFieldFunctions"=>true,"getDumpOutputs"=>true,"getDumpFormats"=>true,"getSettingsRows"=>true,];private$plugins;private$hooks=[];function
__construct(Origin$b,array$ni){$this->plugins=$ni;foreach(get_class_methods('\AdminNeo\Origin')as$Gg){$this->hooks[$Gg]=[];if(!(isset(self::$InternalMethods[$Gg])?self::$InternalMethods[$Gg]:false)){foreach($ni
as$mi){if(method_exists($mi,$Gg))$this->hooks[$Gg][]=$mi;}}if(isset(self::$AppendMethods[$Gg])?self::$AppendMethods[$Gg]:false)array_unshift($this->hooks[$Gg],$b);else$this->hooks[$Gg][]=$b;}}function
getPlugins(){return$this->plugins;}function
__call($A,array$E){$Ja=isset(self::$AppendMethods[$A])?self::$AppendMethods[$A]:false;$J=$Ja?[]:null;assert(isset($this->hooks[$A]),"Calling unknown plugin method: $A");foreach($this->hooks[$A]as$mi){$Y=call_user_func_array([$mi,$A],$E);if($Y!==null){if($Ja)$J+=$Y;else
return$Y;}}return$J;}function
updateCspHeader(array&$qc){$this->__call(__FUNCTION__,[&$qc]);}function
detectJson($Sd,&$Y,$xi=null){return$this->__call(__FUNCTION__,[$Sd,&$Y,$xi]);}}class
