fields($R){$mg=Connection::get()->isMariaDB();$K=[];foreach(get_rows("SELECT * FROM information_schema.COLUMNS WHERE TABLE_SCHEMA = DATABASE() AND TABLE_NAME = ".q($R)." ORDER BY ORDINAL_POSITION")as$L){$l=$L["COLUMN_NAME"];$U=preg_replace('~\s?/\*.+\*/~U',"",$L["COLUMN_TYPE"]);$Jd=$L["EXTRA"];preg_match('~^(VIRTUAL|PERSISTENT|STORED)~',$Jd,$te);preg_match('~^([^( ]+)(?:\((.+)\))?( unsigned)?( zerofill)?$~',$U,$Fl);$j=$mg&&$L["COLUMN_DEFAULT"]=="NULL"?null:$L["COLUMN_DEFAULT"];if($j!==null){$wf=preg_match('~(text|json)~',$Fl[1]);if(!$mg&&$wf)$j=preg_replace("~^(_\w+)?('.*')$~",'\2',stripslashes($j));if($mg||$wf){$j=preg_replace_callback("~^'(.*)'$~",function($_){return
stripslashes(str_replace("''","'",$_[1]));},$j);}if(!$mg&&preg_match('~binary~',$Fl[1])&&preg_match('~^0x(\w*)$~',$j,$_))$j=pack("H*",$_[1]);}$ve=$L["GENERATION_EXPRESSION"];if(!$mg)$ve=preg_replace("~(^|,|\()(_\w+)?('.*')($|,|\))~",'\1\3\4',stripslashes($ve));$K[$l]=["field"=>$l,"full_type"=>$U,"type"=>$Fl[1],"length"=>$Fl[2],"unsigned"=>ltrim($Fl[3].$Fl[4]),"default"=>($te?$ve:$j),"null"=>($L["IS_NULLABLE"]=="YES"),"auto_increment"=>($Jd=="auto_increment"),"on_update"=>(preg_match('~\bon update (\w+)~i',$Jd,$Fl)?$Fl[1]:""),"collation"=>$L["COLLATION_NAME"],"privileges"=>array_flip(explode(",",$L["PRIVILEGES"]))+["where"=>1,"order"=>1],"comment"=>$L["COLUMN_COMMENT"],"primary"=>($L["COLUMN_KEY"]=="PRI"),"generated"=>($te[1]=="PERSISTENT"?"STORED":$te[1]),];}return$K;}function
indexes($R,$f=null){$K=[];foreach(get_rows("SHOW INDEX FROM ".table($R),$f)as$L){$A=$L["Key_name"];$K[$A]["type"]=($A=="PRIMARY"?"PRIMARY":($L["Index_type"]=="FULLTEXT"?"FULLTEXT":($L["Non_unique"]?($L["Index_type"]=="SPATIAL"?"SPATIAL":"INDEX"):"UNIQUE")));$K[$A]["columns"][]=$L["Column_name"];$K[$A]["lengths"][]=($L["Index_type"]=="SPATIAL"?null:$L["Sub_part"]);$K[$A]["descs"][]=null;}return$K;}function
foreign_keys($R){static$fi='(?:`(?:[^`]|``)+`|"(?:[^"]|"")+")';$K=[];$mc=Connection::get()->getValue("SHOW CREATE TABLE ".table($R),1);if($mc){$qh=implode("|",Driver::get()->getOnActions());preg_match_all("~CONSTRAINT ($fi) FOREIGN KEY ?\\(((?:$fi,? ?)+)\\) REFERENCES ($fi)(?:\\.($fi))? \\(((?:$fi,? ?)+)\\)(?: ON DELETE ($qh))?(?: ON UPDATE ($qh))?~",$mc,$_,PREG_SET_ORDER);foreach($_
as$z){preg_match_all("~$fi~",$z[2],$fk);preg_match_all("~$fi~",$z[5],$Xk);$K[idf_unescape($z[1])]=["db"=>idf_unescape($z[4]!=""?$z[3]:$z[4]),"table"=>idf_unescape($z[4]!=""?$z[4]:$z[3]),"source"=>array_map('AdminNeo\idf_unescape',$fk[0]),"target"=>array_map('AdminNeo\idf_unescape',$Xk[0]),"on_delete"=>($z[6]?:"RESTRICT"),"on_update"=>($z[7]?:"RESTRICT"),];}}return$K;}function
