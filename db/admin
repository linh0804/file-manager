function
__construct(Connection$f,$b){parent::__construct($f,$b);$this->types=[lang(110)=>["tinyint"=>3,"smallint"=>5,"mediumint"=>8,"int"=>10,"bigint"=>20,"decimal"=>66,"float"=>12,"double"=>21,],lang(111)=>["date"=>10,"datetime"=>19,"timestamp"=>19,"time"=>10,"year"=>4,],lang(112)=>["char"=>255,"varchar"=>65535,"tinytext"=>255,"text"=>65535,"mediumtext"=>16777215,"longtext"=>4294967295,],lang(113)=>["enum"=>65535,"set"=>64,],lang(114)=>["bit"=>20,"binary"=>255,"varbinary"=>65535,"tinyblob"=>255,"blob"=>65535,"mediumblob"=>16777215,"longblob"=>4294967295,],lang(115)=>["geometry"=>0,"point"=>0,"linestring"=>0,"polygon"=>0,"multipoint"=>0,"multilinestring"=>0,"multipolygon"=>0,"geometrycollection"=>0,],];$this->unsigned=["unsigned","zerofill","unsigned zerofill"];$ng=$f->isMariaDB();if($f->isMinVersion($ng?"10.2":"5.7"))$this->generated=["STORED","VIRTUAL"];$this->operators=["=","<",">","<=",">=","!=","LIKE","LIKE %%","NOT LIKE","IN","NOT IN","FIND_IN_SET","IS NULL","IS NOT NULL","REGEXP","NOT REGEXP","SQL",];$this->likeOperator="LIKE %%";$this->regexpOperator="REGEXP";$this->functions=["char_length","lower","upper","round","floor","ceil","date","from_unixtime","unix_timestamp","sec_to_time","time_to_sec",];$this->grouping=["sum","min","max","avg","count","count distinct","group_concat",];$this->editFunctions=[["char"=>"md5/sha1/password/encrypt/uuid","binary"=>"md5/sha1","date|time"=>"now",],[number_type()=>"+/-","date"=>"+ interval/- interval","time"=>"addtime/subtime","char|text"=>"concat",]];if($f->isMinVersion($ng?"10.2":"5.7.8"))$this->types[lang(112)]["json"]=4294967295;if($ng&&$f->isMinVersion("10.7")){$this->types[lang(112)]["uuid"]=128;$this->editFunctions[0]['uuid']='uuid';}if($f->isMinVersion("9")){$this->types[lang(110)]["vector"]=16383;$this->editFunctions[0]['vector']='string_to_vector';}$this->systemDatabases=["mysql","information_schema","performance_schema","sys"];}function
