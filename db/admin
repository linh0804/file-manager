false;return(object)["name"=>$this->resource->columnName($d),"type"=>$U,"charsetnr"=>($U==SQLITE3_BLOB?63:0),];}}}elseif(extension_loaded("pdo_sqlite")){define("AdminNeo\DRIVER_EXTENSION","PDO_SQLite");abstract
