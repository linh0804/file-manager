.="\nCREATE TRIGGER ".idf_escape($ml['Trigger'])." $ml[Timing] $ml[Event] ON ".idf_escape($P["nspname"]).".".idf_escape($P['Name'])." $ml[Type] $ml[Statement];;\n";}return$I;}function
