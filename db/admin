Connection{private$manager;private$dbName;function
open($N,$V,$D,$wc="",$Ta=null){$this->server_info=MONGODB_VERSION;$ph=[];if($V.$D!=""){$ph["username"]=$V;$ph["password"]=$D;}if($wc!="")$ph["db"]=$wc;if($Ta)$ph["authSource"]=$Ta;$this->manager=new
Manager($N,$ph);$this->dbName=$wc;$this->executeCommand(['ping'=>1]);return
true;}function
executeCommand(array$Ob){try{return$this->manager->executeCommand($this->dbName,new
Command($Ob));}catch(\MongoDB\Driver\Exception\Exception$ud){$this->error=$ud->getMessage();return[];}}function
executeQuery($Kg,Query$F,$ph=null){try{return$this->manager->executeQuery($Kg,$F,$ph);}catch(\MongoDB\Driver\Exception\Exception$ud){$this->error=$ud->getMessage();return[];}}function
executeBulkWrite($Kg,BulkWrite$ib,$kc){try{$Vi=$this->manager->executeBulkWrite($Kg,$ib);$this->affected_rows=$Vi->$kc();return
true;}catch(Exception$Yc){$this->error=$Yc->getMessage();return
false;}}function
query($F,$wl=false){return
false;}function
