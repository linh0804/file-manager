as$X){if($X[0]!="INDEX")$mc[]=($X[2]=="DROP"?"\nDROP CONSTRAINT ".idf_escape($X[1]):"\nADD".($X[1]!=""?" CONSTRAINT ".idf_escape($X[1]):"")." $X[0] ".($X[0]=="PRIMARY"?"KEY ":"")."(".implode(", ",$X[2]).")");elseif($X[2]=="DROP")$Wc[]=idf_escape($X[1]);else$Hi[]="CREATE INDEX ".idf_escape($X[1]!=""?$X[1]:uniqid($R."_"))." ON ".table($R)." (".implode(", ",$X[2]).")";}if($mc)array_unshift($Hi,"ALTER TABLE ".table($R).implode(",",$mc));if($Wc)array_unshift($Hi,"DROP INDEX ".implode(", ",$Wc));foreach($Hi
