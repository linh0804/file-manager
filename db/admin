rename_database($A,$Fb){$K=false;if(create_database($A,$Fb)){$T=[];$jm=[];foreach(tables_list()as$R=>$U){if($U=='VIEW')$jm[]=$R;else$T[]=$R;}$K=(!$T&&!$jm)||move_tables($T,$jm,$A);drop_databases($K?[DB]:[]);}return$K;}function
auto_increment(){$Sa=" PRIMARY KEY";if($_GET["create"]!=""&&$_POST["auto_increment_col"]){foreach(indexes($_GET["create"])as$u){if(in_array($_POST["fields"][$_POST["auto_increment_col"]]["orig"],$u["columns"],true)){$Sa="";break;}if($u["type"]=="PRIMARY")$Sa=" UNIQUE";}}return" AUTO_INCREMENT$Sa";}function
alter_table($R,$A,$m,$he,$Ob,$nd,$Fb,$Ra,$Yh){$c=[];foreach($m
as$l){if($l[1]){$j=$l[1][3];if(str_contains($j," GENERATED")){$l[1][3]=Connection::get()->isMariaDB()?"":$l[1][2];$l[1][2]=$j;}$c[]=($R!=""?($l[0]!=""?"CHANGE ".idf_escape($l[0]):"ADD"):" ")." ".implode($l[1]).($R!=""?$l[2]:"");}else$c[]="DROP ".idf_escape($l[0]);}$c=array_merge($c,$he);$tk=($Ob!==null?" COMMENT=".q($Ob):"").($nd?" ENGINE=".q($nd):"").($Fb?" COLLATE ".q($Fb):"").($Ra!=""?" AUTO_INCREMENT=$Ra":"");if($R=="")return
queries("CREATE TABLE ".table($A)." (\n".implode(",\n",$c)."\n)$tk$Yh");if($R!=$A)$c[]="RENAME TO ".table($A);if($tk)$c[]=ltrim($tk);return($c||$Yh?queries("ALTER TABLE ".table($R)."\n".implode(",\n",$c).$Yh):true);}function
alter_indexes($R,$c){foreach($c
as$w=>$X)$c[$w]=($X[2]=="DROP"?"\nDROP INDEX ".idf_escape($X[1]):"\nADD $X[0] ".($X[0]=="PRIMARY"?"KEY ":"").($X[1]!=""?idf_escape($X[1])." ":"")."(".implode(", ",$X[2]).")");return
queries("ALTER TABLE ".table($R).implode(",",$c));}function
truncate_tables($T){return
apply_queries("TRUNCATE TABLE",$T);}function
drop_views($jm){return
queries("DROP VIEW ".implode(", ",array_map('AdminNeo\table',$jm)));}function
