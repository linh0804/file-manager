printTableStructure(array$m){global$fk,$w;echo"<div class='scrollable'>\n","<table class='nowrap'>\n","<thead><tr><th>",lang(25),"</th><td>",lang(26),"</td><td>",lang(27),"</td>",(support("comment")?"<td>".lang(28)."</td>":""),"</thead>\n";foreach($m
as$l){echo"<tr>","<th>",h($l["field"]),"</th>","<td>";$T=h($l["full_type"]);if(in_array($T,(array)$fk[lang(29)]))echo"<a href='".h(ME.'type='.urlencode($T))."'>$T</a>";else
echo$T;if($l["null"])echo" <i>NULL</i>";if($l["auto_increment"])echo" <i>".lang(30)."</i>";$Ec=h($l["default"]);if(isset($l["default"]))echo" <span title='".lang(31)."'>[<b>",$l["generated"]?"<code class='jush-$w'>$Ec</code>":$Ec,"</b>]</span>";echo"</td>","<td>",h($l["collation"]),"</td>";if(support("comment"))echo"<td>",$this->admin->formatComment($l["comment"]),"</td>";echo"\n";}echo"</table>\n","</div>\n";}function
printTablePartitions(array$Kh){$Ij=$Kh["partition_by"]=="RANGE"||$Kh["partition_by"]=="LIST";echo"<p>","<code>{$Kh["partition_by"]} ({$Kh["partition"]})</code>";if(!$Ij)echo" ".lang(32).": ".h($Kh["partitions"]);echo"</p>";if($Ij){echo"<table>\n","<thead><tr><th>".lang(33)."</th><td>".lang(34)."</td></tr></thead>\n";foreach($Kh["partition_names"]as$x=>$B)echo"<tr><th>".h($B)."</th><td>".h($Kh["partition_values"][$x])."\n";echo"</table>\n";}}function
printTableIndexes(array$v){echo"<table>\n","<thead><tr><th>".lang(26)."</th><td>".lang(25)." (".lang(35).")</td></tr></thead>\n";foreach($v
as$B=>$u){ksort($u["columns"]);$pi=[];foreach($u["columns"]as$x=>$X)$pi[]="<i>".h($X)."</i>".($u["lengths"][$x]?"(".$u["lengths"][$x].")":"").($u["descs"][$x]?" DESC":"");echo"<tr title='".h($B)."'><th>$u[type]<td>".implode(", ",$pi)."\n";}echo"</table>\n";}function
printSelectionColumns(array$K,array$d){global$pe,$ze;print_fieldset_start("select",lang(36),"columns",(bool)$K,true);$_GET["columns"][""]=[];$r=0;foreach($_GET["columns"]as$x=>$X){if($x!=""&&(isset($X["col"])?$X["col"]:null)=="")continue;$c=select_input("name='columns[$r][col]'",$d,isset($X["col"])?$X["col"]:null,$x!==""?"selectFieldChange":"selectAddRow");echo"<div ",($x!=""?"":"class='no-sort'"),">",icon("handle","handle jsonly");if($pe||$ze)echo"<select name='columns[$r][fun]'>",optionlist([-1=>""]+array_filter([lang(37)=>$pe,lang(38)=>$ze]),$X["fun"]),"</select>",help_script_command("value && value.replace(/ |\$/, '(') + ')'",true),script("qsl('select').onchange = (event) => { ".($x!==""?"":" qsl('select, input:not(.remove)', event.target.parentNode).onchange();")." };",""),"($c)";else
echo$c;echo" <button class='button light remove jsonly' title='".h(lang(39))."'>",icon_solo("remove"),"</button>",script("qsl('#fieldset-select .remove').onclick = selectRemoveRow;",""),"</div>\n";$r++;}print_fieldset_end("select",true);}function
printSelectionSearch(array$Z,array$d,array$v){print_fieldset_start("search",lang(40),"search",(bool)$Z);foreach($v
