echo$d;echo" <button class='button light remove jsonly' title='".h(lang(38))."'>",icon_solo("remove"),"</button>",script("qsl('#fieldset-select .remove').onclick = selectRemoveRow;",""),"</div>\n";$r++;}print_fieldset_end("select",true);}function
