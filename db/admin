auto_increment(){$Ta=" PRIMARY KEY";if($_GET["create"]!=""&&$_POST["auto_increment_col"]){foreach(indexes($_GET["create"])as$u){if(in_array($_POST["fields"][$_POST["auto_increment_col"]]["orig"],$u["columns"],true)){$Ta="";break;}if($u["type"]=="PRIMARY")$Ta=" UNIQUE";}}return" AUTO_INCREMENT$Ta";}function
alter_table($R,$A,$m,$je,$Pb,$pd,$Gb,$Sa,$Zh){$c=[];foreach($m
as$l){if($l[1]){$j=$l[1][3];if(str_contains($j," GENERATED")){$l[1][3]=Connection::get()->isMariaDB()?"":$l[1][2];$l[1][2]=$j;}$c[]=($R!=""?($l[0]!=""?"CHANGE ".idf_escape($l[0]):"ADD"):" ")." ".implode($l[1]).($R!=""?$l[2]:"");}else$c[]="DROP ".idf_escape($l[0]);}$c=array_merge($c,$je);$wk=($Pb!==null?" COMMENT=".q($Pb):"").($pd?" ENGINE=".q($pd):"").($Gb?" COLLATE ".q($Gb):"").($Sa!=""?" AUTO_INCREMENT=$Sa":"");if($R=="")return
queries("CREATE TABLE ".table($A)." (\n".implode(",\n",$c)."\n)$wk$Zh");if($R!=$A)$c[]="RENAME TO ".table($A);if($wk)$c[]=ltrim($wk);return($c||$Zh?queries("ALTER TABLE ".table($R)."\n".implode(",\n",$c).$Zh):true);}function
alter_indexes($R,$c){foreach($c
as$w=>$X)$c[$w]=($X[2]=="DROP"?"\nDROP INDEX ".idf_escape($X[1]):"\nADD $X[0] ".($X[0]=="PRIMARY"?"KEY ":"").($X[1]!=""?idf_escape($X[1])." ":"")."(".implode(", ",$X[2]).")");return
queries("ALTER TABLE ".table($R).implode(",",$c));}function
truncate_tables($T){return
apply_queries("TRUNCATE TABLE",$T);}function
drop_views($mm){return
queries("DROP VIEW ".implode(", ",array_map('AdminNeo\table',$mm)));}function
drop_tables($T){return
queries("DROP TABLE ".implode(", ",array_map('AdminNeo\table',$T)));}function
move_tables($T,$mm,$al){$aj=[];foreach($T
as$R)$aj[]=table($R)." TO ".idf_escape($al).".".table($R);if(!$aj||queries("RENAME TABLE ".implode(", ",$aj))){$Gc=[];foreach($mm
as$R)$Gc[table($R)]=view($R);Connection::get()->selectDatabase($al);$i=idf_escape(DB);foreach($Gc
as$A=>$km){if(!queries("CREATE VIEW $A AS ".str_replace(" $i."," ",$km["select"]))||!queries("DROP VIEW $i.$A"))return
false;}return
