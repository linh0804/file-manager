as$v=>$X){if($X[0]=="PRIMARY"){unset($u[$v]);$ke[]="  PRIMARY KEY (".implode(", ",$X[2]).")";}}foreach(foreign_keys($R)as$Mf=>$p){foreach($p["source"]as$v=>$d){if(!$Ph[$d])continue
