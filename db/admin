function
filterListWithWildcards(array$Ol,array$Sd,$Af,array$uk=[]){if(!$Ol||!$Sd)return$Ol;$u=array_search("__system",$Sd);if($u!==false){unset($Sd[$u]);$Sd=array_merge($Sd,$uk);}array_walk($Sd,function(&$Y){$Y=str_replace('\\*',".*",preg_quote($Y,"~"));});$Xh='~^('.implode("|",$Sd).')$~';return$this->filterListWithPattern($Ol,$Xh,$Af);}private
function
filterListWithPattern(array$Ol,$Xh,$Af){$H=[];foreach($Ol
as$w=>$Y){if(is_array($Y)){if($kk=$this->filterListWithPattern($Y,$Xh,$Af))$H[$w]=$kk;}elseif(($Af&&preg_match($Xh,$Y))||(!$Af&&!preg_match($Xh,$Y)))$H[$w]=$Y;}return$H;}abstract
