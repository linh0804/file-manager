alter_table($R,$A,$m,$je,$Pb,$pd,$Gb,$Sa,$Zh){$Xl=($R==""||$je);foreach($m
as$l){if($l[0]!=""||!$l[1]||$l[2]){$Xl=true;break;}}$Ia=[];$Lh=[];foreach($m
as$l){if(!$l[1])continue;if($l[0]!="")$Lh[$l[0]]=$l[1][0];$Ia[]=($Xl?$l[1]:"ADD ".implode($l[1]));}if(!$Xl){foreach($Ia
as$X){if(!queries("ALTER TABLE ".table($R)." $X"))return
false;}if($R!=$A&&!queries("ALTER TABLE ".table($R)." RENAME TO ".table($A)))return
false;}elseif(!recreate_table($R,$A,$Ia,$Lh,$je,$Sa))return
false;if($Sa){queries("BEGIN");queries("UPDATE sqlite_sequence SET seq = $Sa WHERE name = ".q($A));if(!Connection::get()->getAffectedRows())queries("INSERT INTO sqlite_sequence (name, seq) VALUES (".q($A).", $Sa)");queries("COMMIT");}return
true;}function
recreate_table($R,$A,$m,$Lh,$je,$Sa=0,$v=[],$Xc="",$va=""){if($R!=""){if(!$m){foreach(fields($R)as$w=>$l){if($v)$l["auto_increment"]=0;$m[]=process_field($l,$l);$Lh[$w]=idf_escape($w);}}$yi=false;foreach($m
as$l){if($l[6])$yi=true;}$Zc=[];foreach($v
as$w=>$X){if($X[2]=="DROP"){$Zc[$X[1]]=true;unset($v[$w]);}}foreach(indexes($R)as$Jf=>$u){$e=[];foreach($u["columns"]as$w=>$d){if(!isset($Lh[$d]))continue
2;$e[]=$Lh[$d].($u["descs"][$w]?" DESC":"");}if(!$Zc[$Jf]){if($u["type"]!="PRIMARY"||!$yi){$Jf=preg_replace('~^sqlite_~',"",$Jf);$v[]=[$u["type"],$Jf,$e];}}}foreach($v
as$w=>$X){if($X[0]=="PRIMARY"){unset($v[$w]);$je[]="  PRIMARY KEY (".implode(", ",$X[2]).")";}}foreach(foreign_keys($R)as$Jf=>$p){foreach($p["source"]as$w=>$d){if(!$Lh[$d])continue
2;$p["source"][$w]=idf_unescape($Lh[$d]);}if(!isset($je[" $Jf"]))$je[]=" ".format_foreign_key($p);}queries("BEGIN");}foreach($m
as$w=>$l){if(preg_match('~GENERATED~',$l[3]))unset($Lh[array_search($l[0],$Lh)]);$m[$w]="  ".implode($l);}$m=array_merge($m,array_filter($je));foreach(Driver::get()->checkConstraints($R)as$sb){if($sb!=$Xc)$m[]="  CHECK ($sb)";}if($va)$m[]="  CHECK ($va)";$cl=($R==$A?"adminneo_$A":$A);if(!queries("CREATE TABLE ".table($cl)." (\n".implode(",\n",$m)."\n)"))return
false;if($R!=""){if($Lh&&!queries("INSERT INTO ".table($cl)." (".implode(", ",$Lh).") SELECT ".implode(", ",array_map('AdminNeo\idf_escape',array_keys($Lh)))." FROM ".table($R)))return
false;$El=[];foreach(triggers($R)as$Cl=>$nl){$Bl=trigger($Cl);$El[]="CREATE TRIGGER ".idf_escape($Cl)." ".implode(" ",$nl)." ON ".table($A)."\n$Bl[Statement]";}$Sa=$Sa?0:Connection::get()->getValue("SELECT seq FROM sqlite_sequence WHERE name = ".q($R));if(!queries("DROP TABLE ".table($R))||($R==$A&&!queries("ALTER TABLE ".table($cl)." RENAME TO ".table($A)))||!alter_indexes($A,$v))return
false;if($Sa)queries("UPDATE sqlite_sequence SET seq = $Sa WHERE name = ".q($A));foreach($El
as$Bl){if(!queries($Bl))return
false;}queries("COMMIT");}return
true;}function
index_sql($R,$U,$A,$e){return"CREATE $U ".($U!="INDEX"?"INDEX ":"").idf_escape($A!=""?$A:uniqid($R."_"))." ON ".table($R)." $e";}function
alter_indexes($R,$c){foreach($c
as$u){if($u[0]=="PRIMARY"||(preg_match('~^sqlite_~',$u[1])))return
