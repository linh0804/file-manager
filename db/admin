return
xxtea_decrypt_string($g,$w);}$hi=[];if($_COOKIE["neo_permanent"]){foreach(explode(" ",$_COOKIE["neo_permanent"])as$X){list($w)=explode(":",$X);$hi[$w]=$X;}}function
validate_server_input(array&$hi){if(SERVER=="")return;$bi=parse_url(SERVER);if(!$bi)auth_error($hi);if(isset($bi['user'])||isset($bi['pass'])||isset($bi['query'])||isset($bi['fragment']))auth_error($hi);if(isset($bi['scheme'])&&!preg_match('~^(https?)$~i',$bi['scheme']))auth_error($hi);$Ue=(isset($bi['host'])?$bi['host']:'').(isset($bi['path'])?$bi['path']:'');if(!is_server_host_valid($Ue))auth_error($hi);if(isset($bi['port'])&&($bi['port']<1024||$bi['port']>65535))auth_error($hi,lang(125));}if(!function_exists('AdminNeo\is_server_host_valid')){function
