.="ALTER TABLE ONLY ".idf_escape($P['nspname']).".".idf_escape($P['Name'])." ADD CONSTRAINT ".idf_escape($Wd)." $Vd[definition] ".($Vd['deferrable']?'DEFERRABLE':'NOT DEFERRABLE').";\n";return($I?"$I\n":$I);}function
create_sql($R,$Ua,$jk){$Wi=[];$wj=[];$P=table_status($R);if(is_view($P)){$Sl=view($R);return
rtrim("CREATE VIEW ".idf_escape($R)." AS $Sl[select]",";");}$m=fields($R);if(!$P||empty($m))return
false;$I="CREATE TABLE ".idf_escape($P['nspname']).".".idf_escape($P['Name'])." (\n    ";foreach($m
as$l){$Mh=idf_escape($l['field']).' '.$l['full_type'].default_value($l).($l['attnotnull']?" NOT NULL":"");$Wi[]=$Mh;if(preg_match('~nextval\(\'([^\']+)\'\)~',$l['default'],$_)){$vj=$_[1];$K=get_rows((min_version(10)?"SELECT *, cache_size AS cache_value FROM pg_sequences WHERE schemaname = current_schema() AND sequencename = ".q(idf_unescape($vj)):"SELECT * FROM $vj"),null,"-- ");$Xj=reset($K);$wj[]=($jk=="DROP+CREATE"?"DROP SEQUENCE IF EXISTS $vj;\n":"")."CREATE SEQUENCE $vj INCREMENT $Xj[increment_by] MINVALUE $Xj[min_value] MAXVALUE $Xj[max_value]".($Ua&&$Xj['last_value']?" START ".($Xj["last_value"]+1):"")." CACHE $Xj[cache_value];";}}if(!empty($wj))$I=implode("\n\n",$wj)."\n\n$I";$E="";foreach(indexes($R)as$af=>$u){if($u['type']=='PRIMARY'){$E=$af;$Wi[]="CONSTRAINT ".idf_escape($af)." PRIMARY KEY (".implode(', ',array_map('AdminNeo\idf_escape',$u['columns'])).")";}}foreach(Driver::get()->checkConstraints($R)as$Zb=>$dc)$Wi[]="CONSTRAINT ".idf_escape($Zb)." CHECK $dc";$I
.=implode(",\n    ",$Wi)."\n) WITH (oids = ".($P['Oid']?'true':'false').");";if($P['Comment'])$I
.="\n\nCOMMENT ON TABLE ".idf_escape($P['nspname']).".".idf_escape($P['Name'])." IS ".q($P['Comment']).";";foreach($m
as$Od=>$l){if($l['comment'])$I
.="\n\nCOMMENT ON COLUMN ".idf_escape($P['nspname']).".".idf_escape($P['Name']).".".idf_escape($Od)." IS ".q($l['comment']).";";}foreach(get_rows("SELECT indexdef FROM pg_catalog.pg_indexes WHERE schemaname = current_schema() AND tablename = ".q($R).($E?" AND indexname != ".q($E):""),null,"-- ")as$J)$I
.="\n\n$J[indexdef];";return
rtrim($I,';');}function
