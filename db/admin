create_sql($R,$Sa,$zk){$hj=[];$Jj=[];$wk=table_status($R);if(is_view($wk)){$km=view($R);return
rtrim("CREATE VIEW ".idf_escape($R)." AS $km[select]",";");}$m=fields($R);if(!$wk||empty($m))return
