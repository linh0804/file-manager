Pluginer{private
static$InternalMethods=["inject"=>true,"getConfig"=>true,];private
static$AppendMethods=["getErrors"=>true,"getFieldFunctions"=>true,"getDumpOutputs"=>true,"getDumpFormats"=>true];private$plugins;private$hooks=[];function
__construct(Origin$Ha,array$bi){$this->plugins=$bi;foreach(get_class_methods('\AdminNeo\Origin')as$zg){$this->hooks[$zg]=[];if(!(isset(self::$InternalMethods[$zg])?self::$InternalMethods[$zg]:false)){foreach($bi
as$ai){if(method_exists($ai,$zg))$this->hooks[$zg][]=$ai;}}if(isset(self::$AppendMethods[$zg])?self::$AppendMethods[$zg]:false)array_unshift($this->hooks[$zg],$Ha);else$this->hooks[$zg][]=$Ha;}}function
getPlugins(){return$this->plugins;}function
__call($B,array$D){$Ta=isset(self::$AppendMethods[$B])?self::$AppendMethods[$B]:false;$G=$Ta?[]:null;assert(isset($this->hooks[$B]),"Calling unknown plugin method: $B");foreach($this->hooks[$B]as$ai){$Y=call_user_func_array([$ai,$B],$D);if($Y!==null){if($Ta)$G+=$Y;else
return$Y;}}return$G;}function
updateCspHeader(array&$vc){$this->__call(__FUNCTION__,[&$vc]);}function
detectJson($Rd,&$Y,$mi=null){return$this->__call(__FUNCTION__,[$Rd,&$Y,$mi]);}}class
