int32($Qg){while($Qg>=2147483648)$Qg-=4294967296;while($Qg<=-2147483649)$Qg+=4294967296;return(int)$Qg;}function
long2str($W,$pm){$rj='';foreach($W
as$X)$rj
.=pack('V',$X);return$pm?substr($rj,0,end($W)):$rj;}function
str2long($rj,$pm){$W=array_values(unpack('V*',str_pad($rj,4*ceil(strlen($rj)/4),"\0")));if($pm)$W[]=strlen($rj);return$W;}function
xxtea_mx($Dm,$Cm,$Dk,$Ff){return
int32((($Dm>>5&0x7FFFFFF)^$Cm<<2)+(($Cm>>3&0x1FFFFFFF)^$Dm<<4))^int32(($Dk^$Cm)+($Ff^$Dm));}function
xxtea_encrypt_string($li,$w){$w=array_values(unpack("V*",pack("H*",md5($w))));$W=str2long($li,true);$Qg=count($W)-1;$Dm=$W[$Qg];$Cm=$W[0];$Gi=floor(6+52/($Qg+1));$Dk=0;while($Gi-->0){$Dk=int32($Dk+0x9E3779B9);$dd=$Dk>>2&3;for($Qh=0;$Qh<$Qg;$Qh++){$Cm=$W[$Qh+1];$Og=xxtea_mx($Dm,$Cm,$Dk,$w[$Qh&3^$dd]);$Dm=int32($W[$Qh]+$Og);$W[$Qh]=$Dm;}$Cm=$W[0];$Og=xxtea_mx($Dm,$Cm,$Dk,$w[$Qh&3^$dd]);$Dm=int32($W[$Qg]+$Og);$W[$Qg]=$Dm;}return
long2str($W,false);}function
xxtea_decrypt_string($g,$w){$w=array_values(unpack("V*",pack("H*",md5($w))));$W=str2long($g,false);$Qg=count($W)-1;$Dm=$W[$Qg];$Cm=$W[0];$Gi=floor(6+52/($Qg+1));$Dk=int32($Gi*0x9E3779B9);while($Dk){$dd=$Dk>>2&3;for($Qh=$Qg;$Qh>0;$Qh--){$Dm=$W[$Qh-1];$Og=xxtea_mx($Dm,$Cm,$Dk,$w[$Qh&3^$dd]);$Cm=int32($W[$Qh]-$Og);$W[$Qh]=$Cm;}$Dm=$W[$Qg];$Og=xxtea_mx($Dm,$Cm,$Dk,$w[$Qh&3^$dd]);$Cm=int32($W[0]-$Og);$W[0]=$Cm;$Dk=int32($Dk-0x9E3779B9);}return
long2str($W,true);}const
ENCRYPTION_GCM='aes-256-gcm';const
ENCRYPTION_CBC='aes-256-cbc';const
ENCRYPTION_TAG_LENGTH=16;const
ENCRYPTION_HMAC_LENGTH=64;function
generate_iv($Yf){if(function_exists('random_bytes')){try{return
random_bytes($Yf);}catch(Exception$dd){}}return
openssl_random_pseudo_bytes($Yf);}function
hash_key($w){return
substr(hash('sha512',$w,true),0,32);}function
aes_encrypt_string($li,$w){$Gg=PHP_VERSION_ID>=70100&&in_array(ENCRYPTION_GCM,openssl_get_cipher_methods())?ENCRYPTION_GCM:ENCRYPTION_CBC;$w=hash_key($w);$Af=generate_iv(openssl_cipher_iv_length($Gg)?:16);if($Gg==ENCRYPTION_GCM)$Ab=openssl_encrypt($li,$Gg,$w,OPENSSL_RAW_DATA,$Af,$Yk,"",ENCRYPTION_TAG_LENGTH);else{$Ab=openssl_encrypt($li,$Gg,$w,OPENSSL_RAW_DATA,$Af);$Yk=hash_hmac("sha512",$Af.$Ab,$w,true);}if($Ab===false)return
false;return$Af.$Yk.$Ab;}function
aes_decrypt_string($g,$w){$Gg=PHP_VERSION_ID>=70100&&in_array(ENCRYPTION_GCM,openssl_get_cipher_methods())?ENCRYPTION_GCM:ENCRYPTION_CBC;$Bf=openssl_cipher_iv_length($Gg)?:16;$Zk=$Gg==ENCRYPTION_GCM?ENCRYPTION_TAG_LENGTH:ENCRYPTION_HMAC_LENGTH;if(strlen($g)<$Bf+$Zk)return
false;$w=hash_key($w);$Af=substr($g,0,$Bf);$Yk=substr($g,$Bf,$Zk);$Ab=substr($g,$Bf+$Zk);if($Af===false||$Yk===false||$Ab===false)return
false;if($Gg==ENCRYPTION_GCM)return
openssl_decrypt($Ab,$Gg,$w,OPENSSL_RAW_DATA,$Af,$Yk);else{$Te=hash_hmac('sha512',$Af.$Ab,$w,true);if(!hash_equals($Yk,$Te))return
false;return
openssl_decrypt($Ab,$Gg,$w,OPENSSL_RAW_DATA,$Af);}}function
encrypt_string($li,$w){if($li=="")return"";if(extension_loaded('openssl'))return
aes_encrypt_string($li,$w);else
return
xxtea_encrypt_string($li,$w);}function
decrypt_string($g,$w){if($g=="")return"";if(extension_loaded('openssl'))return
aes_decrypt_string($g,$w);else
return
xxtea_decrypt_string($g,$w);}$ji=[];if($_COOKIE["neo_permanent"]){foreach(explode(" ",$_COOKIE["neo_permanent"])as$X){list($w)=explode(":",$X);$ji[$w]=$X;}}function
validate_server_input(array&$ji){if(SERVER=="")return;$ci=parse_url(SERVER);if(!$ci)auth_error($ji);if(isset($ci['user'])||isset($ci['pass'])||isset($ci['query'])||isset($ci['fragment']))auth_error($ji);if(isset($ci['scheme'])&&!preg_match('~^(https?)$~i',$ci['scheme']))auth_error($ji);$We=(isset($ci['host'])?$ci['host']:'').(isset($ci['path'])?$ci['path']:'');if(!is_server_host_valid($We))auth_error($ji);if(isset($ci['port'])&&($ci['port']<1024||$ci['port']>65535))auth_error($ji,lang(124));}if(!function_exists('AdminNeo\is_server_host_valid')){function
