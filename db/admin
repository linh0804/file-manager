printTableStructure(array$m){echo"<div class='scrollable'>\n","<table class='nowrap'>\n","<thead><tr>","<th>",lang(38),"</th>","<td>",lang(39),"</td>","<td>",lang(40),"</td>";if(support("comment"))echo"<td>",lang(41),"</td>";echo"</tr></thead>\n";$bm=Driver::get()->getUserTypes();foreach($m
as$l){echo"<tr>","<th>",h($l["field"]),"</th>","<td>";$U=h($l["full_type"]);if(in_array($U,$bm))echo"<a href='".h(ME.'type='.urlencode($U))."'>$U</a>";else
echo$U;if($l["null"])echo" <i>NULL</i>";if($l["auto_increment"])echo" <i>".lang(42)."</i>";$j=h($l["default"]);if(isset($l["default"]))echo" <span title='".lang(43)."'>[<b>",$l["generated"]?"<code class='jush-".DIALECT."'>$j</code>":$j,"</b>]</span>";echo"</td>","<td>",h($l["collation"]),"</td>";if(support("comment"))echo"<td>",$this->admin->formatComment($l["comment"]),"</td>";echo"\n";}echo"</table>\n","</div>\n";}function
