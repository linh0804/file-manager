table_status($A=""){static$Ce;if($Ce===null)$Ce=Connection::get()->getValue("SELECT 'pg_table_size'::regproc");$I=[];foreach(get_rows("SELECT
	c.relname AS \"Name\",
	CASE c.relkind WHEN 'r' THEN 'table' WHEN 'm' THEN 'materialized view' ELSE 'view' END AS \"Engine\"".($Ce?",
