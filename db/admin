select(Result$J,$f=null,$Dh=[],$x=0){$dg=[];$v=[];$e=[];$cb=[];$Hl=[];$K=[];for($r=0;(!$x||$r<$x)&&($L=$J->fetchRow());$r++){if(!$r){echo"<div class='scrollable'>\n","<table class='nowrap'>\n","<thead><tr>";for($Af=0;$Af<count($L);$Af++){$l=(array)$J->fetchField();$A=$l["name"];$Ch=$l["orgtable"];$Bh=$l["orgname"];$K[$l["table"]]=$Ch;if($Dh&&DIALECT=="sql")$dg[$Af]=($A=="table"?"table=":($A=="possible_keys"?"indexes=":null));elseif($Ch!=""){if(!isset($v[$Ch])){$v[$Ch]=[];foreach(indexes($Ch,$f)as$u){if($u["type"]=="PRIMARY"){$v[$Ch]=array_flip($u["columns"]);break;}}$e[$Ch]=$v[$Ch];}if(isset($e[$Ch][$Bh])){unset($e[$Ch][$Bh]);$v[$Ch][$Bh]=$Af;$dg[$Af]=$Ch;}}if($l["charsetnr"]==63)$cb[$Af]=true;$Hl[$Af]=$l["type"];echo"<th".($Ch!=""||$l["name"]!=$Bh?" title='".h(($Ch!=""?"$Ch.":"").$Bh)."'":"").">".h($A).($Dh?doc_link(['sql'=>"explain-output.html#explain_".strtolower($A),'mariadb'=>"explain/#the-columns-in-explain-select",]):"");}echo"</thead>\n";}echo"<tr>";foreach($L
as$w=>$X){$y="";if(isset($dg[$w])&&!$e[$dg[$w]]){if($Dh&&DIALECT=="sql"){$R=$L[array_search("table=",$dg)];$y=ME.$dg[$w].urlencode($Dh[$R]!=""?$Dh[$R]:$R);}else{$y=ME."edit=".urlencode($dg[$w]);foreach($v[$dg[$w]]as$Db=>$Af)$y
.="&where".urlencode("[".bracket_escape($Db)."]")."=".urlencode($L[$Af]);}}elseif(is_web_url($X))$y=$X;if($X===null)$X="<i>NULL</i>";elseif($cb[$w]&&!is_utf8($X))$X="<i>".lang(37,strlen($X))."</i>";else{$X=h($X);if($Hl[$w]==254)$X="<code>$X</code>";}if($y)$X="<a href='".h($y)."'".(is_web_url($y)?target_blank():'').">$X</a>";$Ab=$Hl[$w]<=9||$Hl[$w]==246?"class='number'":"";echo"<td $Ab>$X</td>";}}echo($r?"</table>\n</div>":"<p class='message'>".lang(81))."\n";return$K;}function
referencable_primary($Bj){$K=[];foreach(table_status('',true)as$Nk=>$R){if($Nk!=$Bj&&fk_support($R)){foreach(fields($Nk)as$l){if($l["primary"]){if($K[$Nk]){unset($K[$Nk]);break;}$K[$Nk]=$l;}}}}return$K;}function
