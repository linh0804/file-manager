as$I)$fm[]="(".implode(", ",$I).")";if($Z){$af=queries("SET IDENTITY_INSERT ".table($R)." ON");$K=queries("MERGE ".table($R)." USING (VALUES\n\t".implode(",\n\t",$fm)."\n) AS source ($e) ON ".implode(" AND ",$Z).($Sl?"\nWHEN MATCHED THEN UPDATE SET ".implode(", ",$Sl):"")."\nWHEN NOT MATCHED THEN INSERT (".implode(", ",array_keys($af?$I:$of)).") VALUES (".($af?$e:implode(", ",$of)).");");if($af)queries("SET IDENTITY_INSERT ".table($R)." OFF");}else$K=queries("INSERT INTO ".table($R)." (".implode(", ",array_keys($I)).") VALUES\n".implode(",\n",$fm));return$K;}function
