.="<a href='#$Zl' class='toggle'>".lang(21).icon_chevron_down()."</a>, ";$_i=support("sql")?lang(22):lang(23);$I
.="<a href='#$Zj' class='toggle'>$_i".icon_chevron_down()."</a>";$I
