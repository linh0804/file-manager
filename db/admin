int32($Eg){while($Eg>=2147483648)$Eg-=4294967296;while($Eg<=-2147483649)$Eg+=4294967296;return(int)$Eg;}function
long2str($W,$Sl){$dj='';foreach($W
as$X)$dj
.=pack('V',$X);return$Sl?substr($dj,0,end($W)):$dj;}function
str2long($dj,$Sl){$W=array_values(unpack('V*',str_pad($dj,4*ceil(strlen($dj)/4),"\0")));if($Sl)$W[]=strlen($dj);return$W;}function
xxtea_mx($hm,$gm,$lk,$yf){return
int32((($hm>>5&0x7FFFFFF)^$gm<<2)+(($gm>>3&0x1FFFFFFF)^$hm<<4))^int32(($lk^$gm)+($yf^$hm));}function
xxtea_encrypt_string($Zh,$x){$x=array_values(unpack("V*",pack("H*",md5($x))));$W=str2long($Zh,true);$Eg=count($W)-1;$hm=$W[$Eg];$gm=$W[0];$xi=floor(6+52/($Eg+1));$lk=0;while($xi-->0){$lk=int32($lk+0x9E3779B9);$dd=$lk>>2&3;for($Eh=0;$Eh<$Eg;$Eh++){$gm=$W[$Eh+1];$Dg=xxtea_mx($hm,$gm,$lk,$x[$Eh&3^$dd]);$hm=int32($W[$Eh]+$Dg);$W[$Eh]=$hm;}$gm=$W[0];$Dg=xxtea_mx($hm,$gm,$lk,$x[$Eh&3^$dd]);$hm=int32($W[$Eg]+$Dg);$W[$Eg]=$hm;}return
long2str($W,false);}function
xxtea_decrypt_string($g,$x){$x=array_values(unpack("V*",pack("H*",md5($x))));$W=str2long($g,false);$Eg=count($W)-1;$hm=$W[$Eg];$gm=$W[0];$xi=floor(6+52/($Eg+1));$lk=int32($xi*0x9E3779B9);while($lk){$dd=$lk>>2&3;for($Eh=$Eg;$Eh>0;$Eh--){$hm=$W[$Eh-1];$Dg=xxtea_mx($hm,$gm,$lk,$x[$Eh&3^$dd]);$gm=int32($W[$Eh]-$Dg);$W[$Eh]=$gm;}$hm=$W[$Eg];$Dg=xxtea_mx($hm,$gm,$lk,$x[$Eh&3^$dd]);$gm=int32($W[0]-$Dg);$W[0]=$gm;$lk=int32($lk-0x9E3779B9);}return
long2str($W,true);}const
ENCRYPTION_GCM='aes-256-gcm';const
ENCRYPTION_CBC='aes-256-cbc';const
ENCRYPTION_TAG_LENGTH=16;const
ENCRYPTION_HMAC_LENGTH=64;function
generate_iv($Sf){if(function_exists('random_bytes')){try{return
random_bytes($Sf);}catch(Exception$dd){}}return
openssl_random_pseudo_bytes($Sf);}function
hash_key($x){return
substr(hash('sha512',$x,true),0,32);}function
aes_encrypt_string($Zh,$x){$Xh=version_compare(PHP_VERSION,"7.1.0")>=0;$zg=$Xh?ENCRYPTION_GCM:ENCRYPTION_CBC;$x=hash_key($x);$tf=generate_iv(openssl_cipher_iv_length($zg)?:16);if($Xh)$Ib=openssl_encrypt($Zh,$zg,$x,OPENSSL_RAW_DATA,$tf,$Dk,"",ENCRYPTION_TAG_LENGTH);else{$Ib=openssl_encrypt($Zh,$zg,$x,OPENSSL_RAW_DATA,$tf);$Dk=hash_hmac("sha512",$tf.$Ib,$x,true);}if($Ib===false)return
false;return$tf.$Dk.$Ib;}function
aes_decrypt_string($g,$x){$Xh=version_compare(PHP_VERSION,"7.1.0")>=0;$zg=$Xh?ENCRYPTION_GCM:ENCRYPTION_CBC;$uf=openssl_cipher_iv_length($zg)?:16;$Ek=$Xh?ENCRYPTION_TAG_LENGTH:ENCRYPTION_HMAC_LENGTH;if(strlen($g)<$uf+$Ek)return
false;$x=hash_key($x);$tf=substr($g,0,$uf);$Dk=substr($g,$uf,$Ek);$Ib=substr($g,$uf+$Ek);if($tf===false||$Dk===false||$Ib===false)return
false;if($Xh)return
openssl_decrypt($Ib,$zg,$x,OPENSSL_RAW_DATA,$tf,$Dk);else{$Me=hash_hmac('sha512',$tf.$Ib,$x,true);if(!hash_equals($Dk,$Me))return
false;return
openssl_decrypt($Ib,$zg,$x,OPENSSL_RAW_DATA,$tf);}}function
encrypt_string($Zh,$x){if($Zh=="")return"";if(extension_loaded('openssl'))return
aes_encrypt_string($Zh,$x);else
return
xxtea_encrypt_string($Zh,$x);}function
