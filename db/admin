as$w=>$X){if(!queries("ALTER TABLE ".table($A)." $w".implode(",",$X)))return
false;}foreach($Sb
as$w=>$X){$Qb=substr($X,9);queries("EXEC sp_dropextendedproperty @name = N'MS_Description', @level0type = N'Schema', @level0name = ".q(get_schema()).", @level1type = N'Table', @level1name = ".q($A).", @level2type = N'Column', @level2name = ".q($w));queries("EXEC sp_addextendedproperty @name = N'MS_Description', @value = ".$Qb.", @level0type = N'Schema', @level0name = ".q(get_schema()).", @level1type = N'Table', @level1name = ".q($A).", @level2type = N'Column', @level2name = ".q($w));}return
