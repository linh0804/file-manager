alter_table($R,$A,$m,$be,$Qb,$jd,$Hb,$Ua,$Qh){$c=[];$Sb=[];$zh=fields($R);foreach($m
as$l){$d=idf_escape($l[0]);$X=$l[1];if(!$X)$c["DROP"][]=" COLUMN $d";else{$X[1]=preg_replace("~( COLLATE )'(\\w+)'~",'\1\2',$X[1]);$Sb[$l[0]]=$X[5];unset($X[5]);if(preg_match('~ AS ~',$X[3]))unset($X[1],$X[2]);if($l[0]=="")$c["ADD"][]="\n  ".implode("",$X).($R==""?substr($be[$X[0]],16+strlen($X[0])):"");else{$j=$X[3];unset($X[3]);unset($X[6]);if($d!=$X[0])queries("EXEC sp_rename ".q(table($R).".$d").", ".q(idf_unescape($X[0])).", 'COLUMN'");$c["ALTER COLUMN ".implode("",$X)][]="";$yh=$zh[$l[0]];if(default_value($yh)!=$j){if($yh["default"]!==null)$c["DROP"][]=" ".idf_escape($yh["default_constraint"]);if($j)$c["ADD"][]="\n $j FOR $d";}}}}if($R=="")return
