as$l){$Vh=idf_escape($l['field']).' '.$l['full_type'].default_value($l).($l['attnotnull']?" NOT NULL":"");$hj[]=$Vh;if(preg_match('~nextval\(\'([^\']+)\'\)~',$l['default'],$_)){$Ij=$_[1];$lk=first(get_rows((Connection::get()->isMinVersion("10")?"SELECT *, cache_size AS cache_value FROM pg_sequences WHERE schemaname = current_schema() AND sequencename = ".q(idf_unescape($Ij)):"SELECT * FROM $Ij"),null,"-- "));$Jj[]=($zk=="DROP+CREATE"?"DROP SEQUENCE IF EXISTS $Ij;\n":"")."CREATE SEQUENCE $Ij INCREMENT $lk[increment_by] MINVALUE $lk[min_value] MAXVALUE $lk[max_value]".($Sa&&$lk['last_value']?" START ".($lk["last_value"]+1):"")." CACHE $lk[cache_value];";}}if(!empty($Jj))$K=implode("\n\n",$Jj)."\n\n$K";$G="";foreach(indexes($R)as$hf=>$u){if($u['type']=='PRIMARY'){$G=$hf;$hj[]="CONSTRAINT ".idf_escape($hf)." PRIMARY KEY (".implode(', ',array_map('AdminNeo\idf_escape',$u['columns'])).")";}}foreach(Driver::get()->checkConstraints($R)as$Zb=>$ec)$hj[]="CONSTRAINT ".idf_escape($Zb)." CHECK $ec";$K
.=implode(",\n    ",$hj)."\n) WITH (oids = ".($wk['Oid']?'true':'false').");";if($wk['Comment'])$K
.="\n\nCOMMENT ON TABLE ".idf_escape($wk['nspname']).".".idf_escape($wk['Name'])." IS ".q($wk['Comment']).";";foreach($m
as$Td=>$l){if($l['comment'])$K
.="\n\nCOMMENT ON COLUMN ".idf_escape($wk['nspname']).".".idf_escape($wk['Name']).".".idf_escape($Td)." IS ".q($l['comment']).";";}foreach(get_rows("SELECT indexdef FROM pg_catalog.pg_indexes WHERE schemaname = current_schema() AND tablename = ".q($R).($G?" AND indexname != ".q($G):""),null,"-- ")as$L)$K
