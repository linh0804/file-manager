Pluginer{private
static$InternalMethods=["inject"=>true,"getConfig"=>true,];private
static$AppendMethods=["getErrors"=>true,"getFieldFunctions"=>true,"getDumpOutputs"=>true,"getDumpFormats"=>true];private$plugins;private$hooks=[];function
__construct(Origin$b,array$ei){$this->plugins=$ei;foreach(get_class_methods('\AdminNeo\Origin')as$_g){$this->hooks[$_g]=[];if(!(isset(self::$InternalMethods[$_g])?self::$InternalMethods[$_g]:false)){foreach($ei
as$di){if(method_exists($di,$_g))$this->hooks[$_g][]=$di;}}if(isset(self::$AppendMethods[$_g])?self::$AppendMethods[$_g]:false)array_unshift($this->hooks[$_g],$b);else$this->hooks[$_g][]=$b;}}function
getPlugins(){return$this->plugins;}function
__call($A,array$C){$Ma=isset(self::$AppendMethods[$A])?self::$AppendMethods[$A]:false;$H=$Ma?[]:null;assert(isset($this->hooks[$A]),"Calling unknown plugin method: $A");foreach($this->hooks[$A]as$di){$Y=call_user_func_array([$di,$A],$C);if($Y!==null){if($Ma)$H+=$Y;else
return$Y;}}return$H;}function
updateCspHeader(array&$pc){$this->__call(__FUNCTION__,[&$pc]);}function
detectJson($Nd,&$Y,$oi=null){return$this->__call(__FUNCTION__,[$Nd,&$Y,$oi]);}}class
